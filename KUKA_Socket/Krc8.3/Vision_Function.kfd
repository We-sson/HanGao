DEFSCRIPT SetBaseNo
Message "SetBaseNo"
SETVAR(FULLPATH[] "$BASE", VALUE[] "{FRAME: X 0,Y 0,Z 0,A 0,B 0,C 0}" )
SETVAR(FULLPATH[] "$ACT_BASE", VALUE[] "%BASE_NO " )
Message "SetBaseNo"
