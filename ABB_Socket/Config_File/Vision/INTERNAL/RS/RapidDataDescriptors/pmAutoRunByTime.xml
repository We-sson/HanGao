<?xml version="1.0" encoding="utf-8"?>
<PpRapidRecordDescriptor xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <DataType>pmAutoRunByTime</DataType>
  <RapidDataComponentDescriptorList>
    <PpRapidDataComponentDescriptor>
      <ComponentName>MenuDataName</ComponentName>
      <ComponentDescription>Name of menudata to be executed</ComponentDescription>
      <AtomicDataType>3</AtomicDataType>
      <StringDataDescriptor>
        <MaxStringLength>80</MaxStringLength>
        <ListDataTypeName>menudata</ListDataTypeName>
        <IsImageFileName>false</IsImageFileName>
        <IsPathProcName>false</IsPathProcName>
      </StringDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>Description</ComponentName>
      <ComponentDescription>Description text for user interface</ComponentDescription>
      <AtomicDataType>3</AtomicDataType>
      <StringDataDescriptor>
        <MaxStringLength>55</MaxStringLength>
        <ListDataTypeName />
        <IsImageFileName>false</IsImageFileName>
        <IsPathProcName>false</IsPathProcName>
      </StringDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>AtHour</ComponentName>
      <ComponentDescription>Execute at hour (disable with -1)</ComponentDescription>
      <AtomicDataType>2</AtomicDataType>
      <NumDataDescriptor>
        <IntegerOnly>true</IntegerOnly>
        <Limits>
          <Enabled>true</Enabled>
          <Min>-1</Min>
          <Max>23</Max>
        </Limits>
        <Enumerations>
          <Items />
          <UseEnumerationsOnly>true</UseEnumerationsOnly>
        </Enumerations>
      </NumDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>AtMinute</ComponentName>
      <ComponentDescription>Execute at minute</ComponentDescription>
      <AtomicDataType>2</AtomicDataType>
      <NumDataDescriptor>
        <IntegerOnly>true</IntegerOnly>
        <Limits>
          <Enabled>true</Enabled>
          <Min>0</Min>
          <Max>59</Max>
        </Limits>
        <Enumerations>
          <Items />
          <UseEnumerationsOnly>true</UseEnumerationsOnly>
        </Enumerations>
      </NumDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>Executed</ComponentName>
      <ComponentDescription>Used by the system</ComponentDescription>
      <AtomicDataType>1</AtomicDataType>
      <BoolDataDescriptor>
        <TrueItem>
          <Name>TRUE</Name>
          <Value>TRUE</Value>
        </TrueItem>
        <FalseItem>
          <Name>FALSE</Name>
          <Value>FALSE</Value>
        </FalseItem>
      </BoolDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>Enabled</ComponentName>
      <ComponentDescription>Enables the function</ComponentDescription>
      <AtomicDataType>1</AtomicDataType>
      <BoolDataDescriptor>
        <TrueItem>
          <Name>TRUE</Name>
          <Value>TRUE</Value>
        </TrueItem>
        <FalseItem>
          <Name>FALSE</Name>
          <Value>FALSE</Value>
        </FalseItem>
      </BoolDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>ConfirmExec</ComponentName>
      <ComponentDescription>Show user message before execution</ComponentDescription>
      <AtomicDataType>1</AtomicDataType>
      <BoolDataDescriptor>
        <TrueItem>
          <Name>TRUE</Name>
          <Value>TRUE</Value>
        </TrueItem>
        <FalseItem>
          <Name>FALSE</Name>
          <Value>FALSE</Value>
        </FalseItem>
      </BoolDataDescriptor>
    </PpRapidDataComponentDescriptor>
  </RapidDataComponentDescriptorList>
</PpRapidRecordDescriptor>