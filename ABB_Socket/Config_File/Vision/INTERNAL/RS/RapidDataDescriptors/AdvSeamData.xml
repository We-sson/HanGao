<?xml version="1.0" encoding="utf-8"?>
<PpRapidRecordDescriptor xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <DataType>AdvSeamData</DataType>
  <RapidDataComponentDescriptorList>
    <PpRapidDataComponentDescriptor>
      <ComponentName>ErrorFunc.RetractWire</ComponentName>
      <ComponentDescription>Specifies if the welding wire should be retracted after the configured number of retries is exceeded</ComponentDescription>
      <AtomicDataType>1</AtomicDataType>
      <BoolDataDescriptor>
        <TrueItem>
          <Name>TRUE</Name>
          <Value>TRUE</Value>
        </TrueItem>
        <FalseItem>
          <Name>FALSE</Name>
          <Value>FALSE</Value>
        </FalseItem>
      </BoolDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>ErrorFunc.RetractTime</ComponentName>
      <ComponentDescription>Time in seconds for wire retract</ComponentDescription>
      <AtomicDataType>2</AtomicDataType>
      <NumDataDescriptor>
        <IntegerOnly>false</IntegerOnly>
        <Limits>
          <Enabled>false</Enabled>
          <Min>0</Min>
          <Max>0</Max>
        </Limits>
        <Enumerations>
          <Items />
          <UseEnumerationsOnly>true</UseEnumerationsOnly>
        </Enumerations>
      </NumDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>ErrorFunc.NuOfRetries</ComponentName>
      <ComponentDescription>The number of automatic restart attempts per seam at welding interrupts</ComponentDescription>
      <AtomicDataType>2</AtomicDataType>
      <NumDataDescriptor>
        <IntegerOnly>false</IntegerOnly>
        <Limits>
          <Enabled>false</Enabled>
          <Min>0</Min>
          <Max>0</Max>
        </Limits>
        <Enumerations>
          <Items />
          <UseEnumerationsOnly>true</UseEnumerationsOnly>
        </Enumerations>
      </NumDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>ErrorFunc.NuOfWeldErrors</ComponentName>
      <ComponentDescription>Numbers of allowed weld errors per seam</ComponentDescription>
      <AtomicDataType>2</AtomicDataType>
      <NumDataDescriptor>
        <IntegerOnly>false</IntegerOnly>
        <Limits>
          <Enabled>false</Enabled>
          <Min>0</Min>
          <Max>0</Max>
        </Limits>
        <Enumerations>
          <Items />
          <UseEnumerationsOnly>true</UseEnumerationsOnly>
        </Enumerations>
      </NumDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>ErrorFunc.SkipForwardDist</ComponentName>
      <ComponentDescription>The distance in mm that the robot moves forward on the current seam relative to the position where it was interrupted</ComponentDescription>
      <AtomicDataType>2</AtomicDataType>
      <NumDataDescriptor>
        <IntegerOnly>false</IntegerOnly>
        <Limits>
          <Enabled>false</Enabled>
          <Min>0</Min>
          <Max>0</Max>
        </Limits>
        <Enumerations>
          <Items />
          <UseEnumerationsOnly>true</UseEnumerationsOnly>
        </Enumerations>
      </NumDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>ErrorFunc.RestartDist</ComponentName>
      <ComponentDescription>The distance that the robot reverses on the current seam relative to the position where it was interrupted</ComponentDescription>
      <AtomicDataType>2</AtomicDataType>
      <NumDataDescriptor>
        <IntegerOnly>false</IntegerOnly>
        <Limits>
          <Enabled>false</Enabled>
          <Min>0</Min>
          <Max>0</Max>
        </Limits>
        <Enumerations>
          <Items />
          <UseEnumerationsOnly>true</UseEnumerationsOnly>
        </Enumerations>
      </NumDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>ScrapeFunc.ScrapeStart</ComponentName>
      <ComponentDescription>Specifies if the robot is to weave at the actual weld start (scrape start). This weaving is automatically interrupted when the arc is ignited</ComponentDescription>
      <AtomicDataType>1</AtomicDataType>
      <BoolDataDescriptor>
        <TrueItem>
          <Name>TRUE</Name>
          <Value>TRUE</Value>
        </TrueItem>
        <FalseItem>
          <Name>FALSE</Name>
          <Value>FALSE</Value>
        </FalseItem>
      </BoolDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>ScrapeFunc.ScrapeDir</ComponentName>
      <ComponentDescription>The angle of direction of the weave for a scrape start. It is specified in degrees, where 0 implies a weave that is carried out at a 90 degrees angle to the direction of the weld.</ComponentDescription>
      <AtomicDataType>2</AtomicDataType>
      <NumDataDescriptor>
        <IntegerOnly>false</IntegerOnly>
        <Limits>
          <Enabled>false</Enabled>
          <Min>0</Min>
          <Max>0</Max>
        </Limits>
        <Enumerations>
          <Items />
          <UseEnumerationsOnly>true</UseEnumerationsOnly>
        </Enumerations>
      </NumDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>ScrapeFunc.ScrapeTime</ComponentName>
      <ComponentDescription>The time (in seconds) it takes for a complete weave cycle for a scrape start</ComponentDescription>
      <AtomicDataType>2</AtomicDataType>
      <NumDataDescriptor>
        <IntegerOnly>false</IntegerOnly>
        <Limits>
          <Enabled>false</Enabled>
          <Min>0</Min>
          <Max>0</Max>
        </Limits>
        <Enumerations>
          <Items />
          <UseEnumerationsOnly>true</UseEnumerationsOnly>
        </Enumerations>
      </NumDataDescriptor>
    </PpRapidDataComponentDescriptor>
    <PpRapidDataComponentDescriptor>
      <ComponentName>ScrapeFunc.ScrapeWidth</ComponentName>
      <ComponentDescription>The width of the weave pattern for a scrape start</ComponentDescription>
      <AtomicDataType>2</AtomicDataType>
      <NumDataDescriptor>
        <IntegerOnly>false</IntegerOnly>
        <Limits>
          <Enabled>false</Enabled>
          <Min>0</Min>
          <Max>0</Max>
        </Limits>
        <Enumerations>
          <Items />
          <UseEnumerationsOnly>true</UseEnumerationsOnly>
        </Enumerations>
      </NumDataDescriptor>
    </PpRapidDataComponentDescriptor>
  </RapidDataComponentDescriptorList>
</PpRapidRecordDescriptor>