                                        <Grid Margin="8">
                                            <StackPanel Orientation="Vertical">


                                                <TextBlock Grid.Column=" 0"
                                                           Text="短期读取变量名列表"
                                                           Margin="0,0,0,8"
                                                           HorizontalAlignment="Center"
                                                           VerticalAlignment="Top"
                                                           FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                           FontSize="18"
                                                           Foreground="{DynamicResource 全局_按钮_黑颜色}" />


                                                <DataGrid AlternatingRowBackground="Gainsboro"
                                                          AlternationCount="2"
                                                          AutoGenerateColumns="False"
                                                          Background="Transparent"
                                                          BorderBrush="{DynamicResource 全局_按钮_黑颜色}"
                                                          BorderThickness="2"
                                                          CanUserAddRows="False"
                                                          CanUserDeleteRows="False"
                                                          CanUserReorderColumns="False"
                                                          CanUserResizeColumns="False"
                                                          CanUserResizeRows="False"
                                                          CanUserSortColumns="False"
                                                          CellStyle="{DynamicResource Point_UI_DataGridCellStyle}"
                                                          ColumnHeaderHeight="30"
                                                          ColumnHeaderStyle="{DynamicResource Point_UI_DataGridColumnHeaderStyle}"
                                                          FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                          FrozenColumnCount="1"
                                                          GridLinesVisibility="Horizontal"
                                                          HeadersVisibility="Column"
                                                          ItemsSource="{Binding On_Read_List}"
                                                          RowHeaderWidth="0"
                                                          RowHeight="25"
                                                          RowStyle="{DynamicResource Point_UI_DataGridRowStyle}"
                                                          SelectionMode="Single"
                                                          SelectionUnit="CellOrRowHeader">

                                                    <DataGrid.Columns>
                                                        <DataGridTemplateColumn Width="auto" IsReadOnly="True">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="序号"
                                                                           Margin="4"
                                                                           HorizontalAlignment="Center"
                                                                           VerticalAlignment="Center"
                                                                           FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                                           FontSize="16"
                                                                           Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding Val_ID}"
                                                                               HorizontalAlignment="Center"
                                                                               VerticalAlignment="Center"
                                                                               FontSize="16" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>

                                                        </DataGridTemplateColumn>

                                                        <DataGridTemplateColumn Width="auto">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="变量名"
                                                                           Margin="4"
                                                                           HorizontalAlignment="Center"
                                                                           VerticalAlignment="Center"
                                                                           FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                                           FontSize="16"
                                                                           Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding Val_Name}"
                                                                               HorizontalAlignment="Left"
                                                                               VerticalAlignment="Center"
                                                                               FontSize="14" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>

                                                        </DataGridTemplateColumn>
                                                        <DataGridTemplateColumn CanUserResize="False">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="变量值"
                                                                           Margin="4"
                                                                           HorizontalAlignment="Center"
                                                                           VerticalAlignment="Center"
                                                                           FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                                           FontSize="16"
                                                                           Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>

                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding Val_Var}"
                                                                               HorizontalAlignment="Left"
                                                                               VerticalAlignment="Center"
                                                                               FontSize="14" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                    </DataGrid.Columns>
                                                </DataGrid>



                                            </StackPanel>



                                        </Grid>














                                    </StackPanel>

                                </Grid>
                            </Expander>



                        </StackPanel>


                    </ScrollViewer>


                </Grid>


            </Grid>
        </Grid>
    </Grid>
</UserControl>
