<UserControl x:Name="UC_Vision"
             x:Class="HanGao.View.FrameShow.Vision"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Charts="clr-namespace:HanGao.View.User_Control.Live_Chart"
             xmlns:Converts_Enum="clr-namespace:Roboto_Socket_Library;assembly=Roboto_Socket_Library"
             xmlns:Enum_Converter="clr-namespace:Halcon_SDK_DLL.WPF_Converter;assembly=Halcon_SDK_DLL"
             xmlns:Halcon="http://schemas.mvtec.com/halcondotnet"
             xmlns:Halcon_Enum="clr-namespace:Halcon_SDK_DLL.Model;assembly=Halcon_SDK_DLL"
             xmlns:MVS="clr-namespace:MvCamCtrl.NET.CameraParams;assembly=MvCamCtrl.Net"
             xmlns:MVS_Enum="clr-namespace:MVS_SDK_Base.Model;assembly=MVS_SDK"
             xmlns:Openfile="clr-namespace:HanGao.View.User_Control.OpenFile"
             xmlns:Process_Enum="clr-namespace:Halcon_SDK_DLL.Halcon_Method;assembly=Halcon_SDK_DLL"
             xmlns:Robot="clr-namespace:Roboto_Socket_Library.Model;assembly=Roboto_Socket_Library"
             xmlns:UC_Working="clr-namespace:HanGao.View.User_Control"
             xmlns:UI_Emum="clr-namespace:HanGao.Model"
             xmlns:Vision_Control="clr-namespace:HanGao.View.User_Control.Vision_Control"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
             xmlns:local="clr-namespace:HanGao.View.FrameShow"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             d:DesignHeight="832"
             d:DesignWidth="1440"
             DataContext="{Binding UC_Visal_Function_VM, Source={StaticResource Locator}}"
             mc:Ignorable="d">

    <i:Interaction.Triggers>

        <i:EventTrigger EventName="Unloaded">
            <i:InvokeCommandAction Command="{Binding CameraDive_List_Exit_Comm}"
                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                   PassEventArgsToCommand="True" />
        </i:EventTrigger>
    </i:Interaction.Triggers>
    <UserControl.Resources>

        <Enum_Converter:Halcon_StringConverter x:Key="Halcon_StringConverter" />
        <Enum_Converter:Halcon_EnumConverter x:Key="Halcon_EnumConverter" />
        <Enum_Converter:EnumDescriptionConverter x:Key="Enum_DescriptionConverter" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
        <Enum_Converter:ValueToObject_Converter x:Key="ValueToObject_Converter" />
        <Enum_Converter:Radio_CheckedToEnumConverter x:Key="Radio_CheckedToEnum" />
        <!--<Enum_Converter:SliderValue_Converter x:Key="SliderValue_Converter" />-->
        <!--<Enum_Converter:MultiTypeValueConverter x:Key="MultiTypeValueConverter" />-->

        <DataTemplate x:Key="Result_ListViewItemTemplate">
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="{Binding}"
                           Margin="0,0,4,0"
                           VerticalAlignment="Center"
                           FontFamily="{DynamicResource 全局_文字_英文字体}"
                           FontSize="12"
                           Foreground="{DynamicResource 全局_按钮_白颜色}" />
            </StackPanel>
        </DataTemplate>
    </UserControl.Resources>

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        <Grid Grid.Column=" 0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition MaxHeight="300" />
            </Grid.RowDefinitions>
            <Grid Grid.Row="0" Grid.Column=" 0"
                  Grid.ColumnSpan=" 2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition Width="70" />
                </Grid.ColumnDefinitions>
                <Grid Grid.Column=" 0" Margin="6,6,3,3">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40" />
                        <RowDefinition />
                    </Grid.RowDefinitions>



                    <Grid Grid.Row=" 0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition />
                            <ColumnDefinition Width=" auto" />
                        </Grid.ColumnDefinitions>
                        <Border Grid.Column=" 0 "
                                Grid.ColumnSpan=" 5"
                                Background="{DynamicResource 全局_按钮_黄颜色}"
                                CornerRadius="5 5 0 0" />
                        <!--  图像窗口按钮  -->
                        <Grid Grid.Column=" 0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <RadioButton Style="{DynamicResource Halcon_Window_RadioButton_Style}" x:Name="Live_Window_Button" Grid.Column=" 0"
                                         IsChecked="False"
                                         Margin="5"
                                         HorizontalContentAlignment="Center"
                                         VerticalContentAlignment="Center"
                                         GroupName="Halcon_Window">
                                <StackPanel Margin="5"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Orientation="Horizontal">
                                    <Image Height=" 24"
                                           Margin="5,0,5,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           Source="/Resources/Vision_相机视角_ioc.png" />
                                    <TextBlock Text="相机视角"
                                               HorizontalAlignment="Center"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_中文字体}"
                                               FontSize="16" />
                                </StackPanel>
                            </RadioButton>
                            <RadioButton Style="{DynamicResource Halcon_Window_RadioButton_Style}" x:Name="Results_Window_Button" Grid.Column=" 1"
                                         IsChecked="False"
                                         Margin="5"
                                         HorizontalContentAlignment="Center"
                                         VerticalContentAlignment="Center"
                                         GroupName="Halcon_Window">
                                <StackPanel Margin="5"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Orientation="Horizontal">
                                    <Image Height=" 24"
                                           Margin="5,0,5,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           Source="/Resources/Vision_图像结果_ioc.png" />
                                    <TextBlock Text="图像结果"
                                               HorizontalAlignment="Center"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                               FontSize="16" />
                                </StackPanel>
                            </RadioButton>
                            <RadioButton Style="{DynamicResource Halcon_Window_RadioButton_Style}" x:Name="Features_Window_Button" Grid.Column=" 2"
                                         IsChecked="True"
                                         Margin="5"
                                         HorizontalContentAlignment="Center"
                                         VerticalContentAlignment="Center"
                                         GroupName="Halcon_Window">
                                <StackPanel Margin="5"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Orientation="Horizontal">
                                    <Image Height=" 20"
                                           Margin="5,0,5,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           Source="/Resources/Vision_特征图像_ioc.png" />
                                    <TextBlock Text="特征图像"
                                               HorizontalAlignment="Center"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                               FontSize="16" />
                                </StackPanel>
                            </RadioButton>

                            <RadioButton Style="{DynamicResource Halcon_Window_RadioButton_Style}" x:Name="Features_3D_Window_Button" Grid.Column=" 3"
                                         IsChecked="False"
                                         Margin="5"
                                         HorizontalContentAlignment="Center"
                                         VerticalContentAlignment="Center"
                                         GroupName="Halcon_Window">
                                <StackPanel Margin="5"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Orientation="Horizontal">
                                    <Image Height=" 20"
                                           Margin="5,0,5,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           Source="/Resources/Vision_特征图像_ioc.png" />
                                    <TextBlock Text="三维图像"
                                               HorizontalAlignment="Center"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                               FontSize="16" />
                                </StackPanel>
                            </RadioButton>

                        </Grid>
                        <!--  图像功能按钮  -->
                        <Grid Grid.Column=" 1"
                              Margin="8,0,8,0"
                              HorizontalAlignment="Right">
                            <Grid.Style>
                                <Style TargetType="Grid">
                                    <Setter Property="Visibility" Value="Collapsed" />
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding ElementName=Results_Window_Button, Path=IsChecked}" Value="true">
                                            <Setter Property="Visibility" Value="Visible" />
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding ElementName=Features_Window_Button, Path=IsChecked}" Value="true">
                                            <Setter Property="Visibility" Value="Visible" />
                                        </DataTrigger>

                                    </Style.Triggers>
                                </Style>
                            </Grid.Style>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Menu Style="{DynamicResource Menu_Style}"
                                  VerticalAlignment="Center"
                                  HorizontalContentAlignment="Center"
                                  VerticalContentAlignment="Center"
                                  Background="{DynamicResource 全局_背景_白颜色}"
                                  FontFamily="{DynamicResource 全局_文字_中文字体}"
                                  FontSize="14">
                                <MenuItem>
                                    <MenuItem.Header>
                                        <TextBlock Text="功能设置"
                                                   VerticalAlignment="Center"
                                                   FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                   FontSize="14" />
                                    </MenuItem.Header>
                                    <MenuItem.Icon>
                                        <Image Source="/Resources/Vision_下拉选择_ioc.png" />
                                    </MenuItem.Icon>
                                    <MenuItem>
                                        <MenuItem.Header>
                                            <TextBlock Text="图像功能"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="14" />
                                        </MenuItem.Header>
                                        <MenuItem IsChecked="False" IsCheckable="True">
                                            <MenuItem.Header>
                                                <TextBlock Text="保存拍照图片本地"
                                                           VerticalAlignment="Center"
                                                           FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                           FontSize="14" />
                                            </MenuItem.Header>
                                        </MenuItem>
                                    </MenuItem>
                                    <MenuItem>
                                        <MenuItem.Header>
                                            <TextBlock Text="相机详细信息"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="14" />
                                        </MenuItem.Header>
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="Click">
                                                <i:InvokeCommandAction Command="{Binding}"
                                                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                       PassEventArgsToCommand="True" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </MenuItem>
                                    <MenuItem>
                                        <MenuItem.Icon>
                                            <Label Grid.Column=" 0"
                                                   Content="&#xe646;"
                                                   Margin="0"
                                                   Padding="0"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontFamily="/Resources/#iconfont"
                                                   FontSize=" 16"
                                                   Foreground="{DynamicResource 全局_按钮_蓝颜色}" />
                                        </MenuItem.Icon>
                                        <MenuItem.Header>
                                            <TextBlock Text="图像结果布局"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="14" />
                                        </MenuItem.Header>
                                        <MenuItem IsChecked="{Binding UI_ImageResults_Window_Switch, Mode=TwoWay, Converter={StaticResource Radio_CheckedToEnum}, ConverterParameter={x:Static UI_Emum:UI_FromWindow_Switch_Enum.One_Window}}" IsCheckable="True">

                                            <MenuItem.Header>
                                                <TextBlock Text="单窗口"
                                                           VerticalAlignment="Center"
                                                           FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                           FontSize="14" />
                                            </MenuItem.Header>
                                        </MenuItem>
                                        <MenuItem IsChecked="{Binding UI_ImageResults_Window_Switch, Mode=TwoWay, Converter={StaticResource Radio_CheckedToEnum}, ConverterParameter={x:Static UI_Emum:UI_FromWindow_Switch_Enum.Two_Window}}" IsCheckable="True">
                                            <MenuItem.Header>
                                                <TextBlock Text="双窗口"
                                                           VerticalAlignment="Center"
                                                           FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                           FontSize="14" />
                                            </MenuItem.Header>
                                        </MenuItem>
                                        <MenuItem IsChecked="{Binding UI_ImageResults_Window_Switch, Mode=TwoWay, Converter={StaticResource Radio_CheckedToEnum}, ConverterParameter={x:Static UI_Emum:UI_FromWindow_Switch_Enum.Four_Window}}" IsCheckable="True">
                                            <MenuItem.Header>
                                                <TextBlock Text="四窗口"
                                                           VerticalAlignment="Center"
                                                           FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                           FontSize="14" />
                                            </MenuItem.Header>
                                        </MenuItem>
                                    </MenuItem>
                                    <MenuItem>
                                        <MenuItem.Icon>
                                            <Label Grid.Column=" 0"
                                                   Content="&#xe646;"
                                                   Margin="0"
                                                   Padding="0"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontFamily="/Resources/#iconfont"
                                                   FontSize=" 16"
                                                   Foreground="{DynamicResource 全局_按钮_蓝颜色}" />
                                        </MenuItem.Icon>
                                        <MenuItem.Header>
                                            <TextBlock Text="特征图像布局"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="14" />
                                        </MenuItem.Header>
                                        <MenuItem IsChecked="{Binding UI_EditImage_Window_Switch, Mode=TwoWay, Converter={StaticResource Radio_CheckedToEnum}, ConverterParameter={x:Static UI_Emum:UI_FromWindow_Switch_Enum.One_Window}}" IsCheckable="True">
                                            <MenuItem.Header>
                                                <TextBlock Text="单窗口"
                                                           VerticalAlignment="Center"
                                                           FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                           FontSize="14" />
                                            </MenuItem.Header>
                                        </MenuItem>
                                        <MenuItem IsChecked="{Binding UI_EditImage_Window_Switch, Mode=TwoWay, Converter={StaticResource Radio_CheckedToEnum}, ConverterParameter={x:Static UI_Emum:UI_FromWindow_Switch_Enum.Two_Window}}" IsCheckable="True">
                                            <MenuItem.Header>
                                                <TextBlock Text="双窗口"
                                                           VerticalAlignment="Center"
                                                           FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                           FontSize="14" />
                                            </MenuItem.Header>
                                        </MenuItem>
                                        <MenuItem IsChecked="{Binding UI_EditImage_Window_Switch, Mode=TwoWay, Converter={StaticResource Radio_CheckedToEnum}, ConverterParameter={x:Static UI_Emum:UI_FromWindow_Switch_Enum.Four_Window}}" IsCheckable="True">
                                            <MenuItem.Header>
                                                <TextBlock Text="四窗口"
                                                           VerticalAlignment="Center"
                                                           FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                           FontSize="14" />
                                            </MenuItem.Header>
                                        </MenuItem>
                                    </MenuItem>


                                </MenuItem>
                            </Menu>




                        </Grid>

                    </Grid>


                    <Grid Grid.Row=" 1">
                        <Grid.Style>
                            <Style TargetType="Grid">
                                <Setter Property="Visibility" Value="Hidden" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding IsChecked, ElementName=Live_Window_Button}" Value="True">
                                        <Setter Property="Visibility" Value="Visible" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Grid.Style>

                        <Halcon:HSmartWindowControlWPF x:Name="Live_Window"
                                                       HorizontalAlignment="Stretch"
                                                       VerticalAlignment="Stretch"
                                                       HorizontalContentAlignment="Center"
                                                       VerticalContentAlignment="Center"
                                                       HDisableAutoResize="True"
                                                       SnapsToDevicePixels="True">
                            <Halcon:HIconicDisplayObjectWPF IconicObject="{Binding Halcon_Window_Display.Live_Window.DisplayImage}" />
                            <Halcon:HIconicDisplayObjectWPF HColor="{Binding Halcon_Window_Display.Live_Window.SetDisplay.SetColor, UpdateSourceTrigger=PropertyChanged}"
                                                            HDraw="{Binding Halcon_Window_Display.Live_Window.SetDisplay.SetDraw, UpdateSourceTrigger=PropertyChanged}"
                                                            IconicObject="{Binding Halcon_Window_Display.Live_Window.DisplayRegion}" />
                            <Halcon:HIconicDisplayObjectWPF HColored="3" IconicObject="{Binding Halcon_Window_Display.Live_Window.DisplayXLD}" />
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="Loaded">
                                    <i:InvokeCommandAction Command="{Binding Initialization_Camera_Window_Comm}"
                                                           CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                           PassEventArgsToCommand="True" />
                                </i:EventTrigger>
                            </i:Interaction.Triggers>

                        </Halcon:HSmartWindowControlWPF>
                    </Grid>
                    <Grid Grid.Row=" 1" ShowGridLines="False">
                        <Grid.Style>
                            <Style TargetType="Grid">
                                <Setter Property="Visibility" Value="Hidden" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding IsChecked, ElementName=Results_Window_Button}" Value="True">
                                        <Setter Property="Visibility" Value="Visible" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Grid.Style>
                        <!--  结果窗口  -->
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition Height="auto" />
                            <RowDefinition />
                        </Grid.RowDefinitions>

                        <!--  Halcon 控件  -->




                        <!--  1  -->
                        <Grid Grid.Row=" 0" Grid.Column="0">
                            <Grid.Style>
                                <Style TargetType="Grid">
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding UI_ImageResults_Window_Switch}" Value="{x:Static UI_Emum:UI_FromWindow_Switch_Enum.One_Window}">

                                            <Setter Property="Grid.RowSpan" Value="3" />
                                            <Setter Property="Grid.ColumnSpan" Value="3" />
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding UI_ImageResults_Window_Switch}" Value="{x:Static UI_Emum:UI_FromWindow_Switch_Enum.Two_Window}">

                                            <Setter Property="Grid.RowSpan" Value="3" />

                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Grid.Style>

                            <Halcon:HSmartWindowControlWPF x:Name="Results_Window_1"
                                                           HorizontalAlignment="Stretch"
                                                           VerticalAlignment="Stretch"
                                                           HorizontalContentAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           SnapsToDevicePixels="True">
                                <Halcon:HIconicDisplayObjectWPF IconicObject="{Binding Halcon_Window_Display.Results_Window_1.DisplayImage}" />
                                <Halcon:HIconicDisplayObjectWPF HColor="{Binding Halcon_Window_Display.Results_Window_1.SetDisplay.SetColor, UpdateSourceTrigger=PropertyChanged}"
                                                                HDraw="{Binding Halcon_Window_Display.Results_Window_1.SetDisplay.SetDraw, UpdateSourceTrigger=PropertyChanged}"
                                                                IconicObject="{Binding Halcon_Window_Display.Results_Window_1.DisplayRegion}" />
                                <Halcon:HIconicDisplayObjectWPF HColor="red" IconicObject="{Binding Halcon_Window_Display.Results_Window_1.DisplayXLD}" />
                                <i:Interaction.Triggers>
                                    <i:EventTrigger EventName="Loaded">
                                        <i:InvokeCommandAction Command="{Binding Initialization_Camera_Window_Comm}"
                                                               CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                               PassEventArgsToCommand="True" />
                                    </i:EventTrigger>
                                </i:Interaction.Triggers>
                            </Halcon:HSmartWindowControlWPF>

                            <Grid x:Name="G1" VerticalAlignment="Bottom">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="auto" />
                                </Grid.ColumnDefinitions>
                                <Border Grid.Column=" 0"
                                        VerticalAlignment="Bottom"
                                        Background="{DynamicResource 全局_按钮_黑颜色}"
                                        Opacity=" 0.8">
                                    <Border.Style>
                                        <Style TargetType="Border">
                                            <Setter Property="Height" Value=" 25" />
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding IsMouseOver, ElementName=G1}" Value="true">
                                                    <Setter Property="Height" Value="150" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Border.Style>
                                </Border>
                                <StackPanel Grid.Column=" 0"
                                            HorizontalAlignment="Left"
                                            Orientation="Horizontal">
                                    <ListView VerticalAlignment="Bottom"
                                              Background="Transparent"
                                              BorderBrush="Transparent"
                                              BorderThickness="0"
                                              ItemContainerStyle="{DynamicResource Vison_Data_ListBoxItemContainerStyle}"
                                              ItemTemplate="{DynamicResource Result_ListViewItemTemplate}"
                                              ItemsSource="{Binding Halcon_Window_Display.Results_Window_1.Find_Result_Show_Window.Text_Arr_UI}"
                                              ScrollViewer.VerticalScrollBarVisibility="Disabled">
                                        <ListView.Style>
                                            <Style TargetType="ListView">
                                                <Setter Property="Height" Value=" 24" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding IsMouseOver, ElementName=G1}" Value="true">
                                                        <Setter Property="Height" Value=" 150" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </ListView.Style>
                                    </ListView>
                                </StackPanel>
                                <Border Grid.Column=" 1"
                                        Height=" 25"
                                        VerticalAlignment="Bottom"
                                        Opacity=" 0.8">
                                    <Border.Style>
                                        <Style TargetType="Border">
                                            <Setter Property="Background" Value="{DynamicResource 全局_按钮_深绿颜色}" />
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding Halcon_Window_Display.Results_Window_1.Find_Result_Show_Window.Find_Score}" Value="0">
                                                    <Setter Property="Background" Value="{DynamicResource 全局_按钮_红颜色}" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Border.Style>
                                </Border>
                                <StackPanel Grid.Column=" 1"
                                            Margin="5"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Bottom"
                                            Orientation="Horizontal">
                                    <TextBlock Text="窗口_1   "
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                               FontSize="12"
                                               Foreground="{DynamicResource 全局_按钮_白颜色}" />
                                    <TextBlock VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                               FontSize="12"
                                               Foreground="{DynamicResource 全局_按钮_白颜色}">
                                        <TextBlock.Style>
                                            <Style TargetType="TextBlock">
                                                <Setter Property="Text" Value="结果 : PASS" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding Halcon_Window_Display.Results_Window_1.Find_Result_Show_Window.Find_Score}" Value="0">
                                                        <Setter Property="Text" Value="结果 : NG" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBlock.Style>
                                    </TextBlock>
                                </StackPanel>
                            </Grid>


                        </Grid>

                        <Grid Grid.Row=" 2" Grid.Column=" 0">
                            <Grid.Style>
                                <Style TargetType="Grid">
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding UI_ImageResults_Window_Switch}" Value="{x:Static UI_Emum:UI_FromWindow_Switch_Enum.Two_Window}">

                                            <Setter Property="Visibility" Value="Collapsed" />
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding UI_ImageResults_Window_Switch}" Value="{x:Static UI_Emum:UI_FromWindow_Switch_Enum.One_Window}">

                                            <Setter Property="Visibility" Value="Collapsed" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Grid.Style>
                            <Halcon:HSmartWindowControlWPF x:Name="Results_Window_2" Grid.Row=" 2" Grid.Column=" 0"
                                                           HorizontalAlignment="Stretch"
                                                           VerticalAlignment="Stretch"
                                                           HorizontalContentAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           SnapsToDevicePixels="True">
                                <Halcon:HIconicDisplayObjectWPF IconicObject="{Binding Halcon_Window_Display.Results_Window_2.DisplayImage}" />
                                <Halcon:HIconicDisplayObjectWPF HColor="{Binding Halcon_Window_Display.Results_Window_2.SetDisplay.SetColor, UpdateSourceTrigger=PropertyChanged}"
                                                                HDraw="{Binding Halcon_Window_Display.Results_Window_2.SetDisplay.SetDraw, UpdateSourceTrigger=PropertyChanged}"
                                                                IconicObject="{Binding Halcon_Window_Display.Results_Window_2.DisplayRegion}" />
                                <Halcon:HIconicDisplayObjectWPF HColor="red" IconicObject="{Binding Halcon_Window_Display.Results_Window_2.DisplayXLD}" />
                                <i:Interaction.Triggers>
                                    <i:EventTrigger EventName="Loaded">
                                        <i:InvokeCommandAction Command="{Binding Initialization_Camera_Window_Comm}"
                                                               CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                               PassEventArgsToCommand="True" />
                                    </i:EventTrigger>
                                </i:Interaction.Triggers>
                                <!--<Halcon:HSmartWindowControlWPF.OpacityMask>
                                    <VisualBrush Visual="{Binding ElementName=Mask1}" />
                                </Halcon:HSmartWindowControlWPF.OpacityMask>-->
                            </Halcon:HSmartWindowControlWPF>
                            <Grid x:Name="G2" VerticalAlignment="Bottom">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
