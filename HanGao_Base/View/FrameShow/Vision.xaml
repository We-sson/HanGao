<UserControl x:Name="UC_Vision"
             x:Class="HanGao.View.FrameShow.Vision"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Charts="clr-namespace:HanGao.View.User_Control.Live_Chart"
             xmlns:Enum="clr-namespace:Halcon_SDK_DLL.WPF_Converter;assembly=Halcon_SDK_DLL"
             xmlns:Halcon="http://schemas.mvtec.com/halcondotnet"
             xmlns:Halcon_Enum="clr-namespace:Halcon_SDK_DLL.Model;assembly=Halcon_SDK_DLL"
             xmlns:MVS="clr-namespace:MvCamCtrl.NET.CameraParams;assembly=MvCamCtrl.Net"
             xmlns:MVS_Enum="clr-namespace:MVS_SDK_Base.Model;assembly=MVS_SDK"
             xmlns:UC_Working="clr-namespace:HanGao.View.User_Control"
             xmlns:Vision_Control="clr-namespace:HanGao.View.User_Control.Vision_Control"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
             xmlns:local="clr-namespace:HanGao.View.FrameShow"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="832"
             d:DesignWidth="1440"
             DataContext="{Binding UC_Visal_Function_VM, Source={StaticResource Locator}}"
             mc:Ignorable="d">
    <UserControl.Resources>

        <Enum:Halcon_StringConverter x:Key="Halcon_StringConverter" />
        <Enum:Halcon_EnumConverter x:Key="Halcon_EnumConverter" />

        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />

        <DataTemplate x:Key="Result_ListViewItemTemplate">
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="{Binding}"
                           Margin="0,0,4,0"
                           VerticalAlignment="Center"
                           FontFamily="{DynamicResource 全局_文字_英文字体}"
                           FontSize="12"
                           Foreground="{DynamicResource 全局_按钮_白颜色}" />
            </StackPanel>
        </DataTemplate>
    </UserControl.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        <Grid Grid.Column=" 0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition MaxHeight="300" />
            </Grid.RowDefinitions>
            <Grid Grid.Row="0" Grid.Column=" 0"
                  Grid.ColumnSpan=" 2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition Width="70" />
                </Grid.ColumnDefinitions>
                <Grid Grid.Column=" 0" Margin="6,6,3,3">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40" />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid Grid.Row=" 0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition />
                            <ColumnDefinition Width=" auto" />
                        </Grid.ColumnDefinitions>
                        <Border Grid.Column=" 0 "
                                Grid.ColumnSpan=" 5"
                                Background="{DynamicResource 全局_按钮_黄颜色}"
                                CornerRadius="5 5 0 0" />
                        <!--  图像窗口按钮  -->
                        <Grid Grid.Column=" 0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <RadioButton Style="{DynamicResource Halcon_Window_RadioButton_Style}" x:Name="Live_Window_Button" Grid.Column=" 0"
                                         IsChecked="False"
                                         Margin="5"
                                         HorizontalContentAlignment="Center"
                                         VerticalContentAlignment="Center"
                                         GroupName="Halcon_Window">
                                <StackPanel Margin="5"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Orientation="Horizontal">
                                    <Image Height=" 24"
                                           Margin="5,0,5,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           Source="/Resources/Vision_相机视角_ioc.png" />
                                    <TextBlock Text="相机视角"
                                               HorizontalAlignment="Center"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                               FontSize="16" />
                                </StackPanel>
                            </RadioButton>
                            <RadioButton Style="{DynamicResource Halcon_Window_RadioButton_Style}" x:Name="Results_Window_Button" Grid.Column=" 1"
                                         IsChecked="True"
                                         Margin="5"
                                         HorizontalContentAlignment="Center"
                                         VerticalContentAlignment="Center"
                                         GroupName="Halcon_Window">
                                <StackPanel Margin="5"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Orientation="Horizontal">
                                    <Image Height=" 24"
                                           Margin="5,0,5,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           Source="/Resources/Vision_图像结果_ioc.png" />
                                    <TextBlock Text="图像结果"
                                               HorizontalAlignment="Center"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                               FontSize="16" />
                                </StackPanel>
                            </RadioButton>
                            <RadioButton Style="{DynamicResource Halcon_Window_RadioButton_Style}" x:Name="Features_Window_Button" Grid.Column=" 2"
                                         IsChecked="False"
                                         Margin="5"
                                         HorizontalContentAlignment="Center"
                                         VerticalContentAlignment="Center"
                                         GroupName="Halcon_Window">
                                <StackPanel Margin="5"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Orientation="Horizontal">
                                    <Image Height=" 20"
                                           Margin="5,0,5,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           Source="/Resources/Vision_特征图像_ioc.png" />
                                    <TextBlock Text="特征图像"
                                               HorizontalAlignment="Center"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                               FontSize="16" />
                                </StackPanel>
                            </RadioButton>
                        </Grid>
                        <!--  图像功能按钮  -->
                        <Grid Grid.Column=" 1" HorizontalAlignment="Right">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Menu Style="{DynamicResource Menu_Style}"
                                  Padding="4"
                                  VerticalAlignment="Center"
                                  HorizontalContentAlignment="Center"
                                  VerticalContentAlignment="Center"
                                  Background="{DynamicResource 全局_按钮_黑颜色}"
                                  DataContext="{Binding Source={StaticResource Locator}, Path=Global_Seting}"
                                  FontFamily="{DynamicResource 全局_文字_数字字体}"
                                  FontSize="16">
                                <MenuItem>
                                    <MenuItem.Header>
                                        <TextBlock Text="功能设置"
                                                   VerticalAlignment="Center"
                                                   FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                   FontSize="16" />
                                    </MenuItem.Header>
                                    <MenuItem.Icon>
                                        <Image Source="/Resources/Vision_下拉选择_ioc.png" />
                                    </MenuItem.Icon>
                                    <MenuItem>
                                        <MenuItem.Header>
                                            <TextBlock Text="图像功能"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="14" />
                                        </MenuItem.Header>
                                        <MenuItem IsChecked="{Binding Global_Seting.IsVisual_image_saving}" IsCheckable="True">
                                            <MenuItem.Header>
                                                <TextBlock Text="保存拍照图片本地"
                                                           VerticalAlignment="Center"
                                                           FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                           FontSize="14" />
                                            </MenuItem.Header>
                                        </MenuItem>
                                    </MenuItem>
                                    <MenuItem>
                                        <MenuItem.Header>
                                            <TextBlock Text="相机详细信息"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="14" />
                                        </MenuItem.Header>
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="Click">
                                                <i:InvokeCommandAction Command="{Binding Camera_Info_Display_Comm}"
                                                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                       PassEventArgsToCommand="True" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </MenuItem>
                                </MenuItem>
                            </Menu>
                            <Grid Grid.Column="1">
                                <ToggleButton Style="{DynamicResource Window_ToggleButtonStyle}" x:Name="Window_Switch"
                                              IsChecked="True"
                                              Height=" 35"
                                              Margin="4,0,4,0"
                                              Padding="0"
                                              Background="{DynamicResource 全局_背景_白颜色}"
                                              BorderBrush="{DynamicResource 全局_背景_白颜色}"
                                              BorderThickness="2">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Label Grid.Column=" 0"
                                               Content="&#xe627;"
                                               Margin="0"
                                               Padding="12,0,12,0"
                                               HorizontalAlignment="Center"
                                               VerticalAlignment="Center"
                                               FontFamily="/Resources/#iconfont"
                                               FontSize=" 20">
                                            <Label.Style>
                                                <Style TargetType="Label">
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding IsChecked, ElementName=Window_Switch}" Value="false">
                                                            <Setter Property="Foreground" Value="{DynamicResource 全局_按钮_蓝颜色}" />
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Label.Style>
                                        </Label>

                                        <Label Grid.Column=" 1"
                                               Content="&#xe62d;"
                                               Margin="0"
                                               Padding="12,0,12,0"
                                               HorizontalAlignment="Center"
                                               VerticalAlignment="Center"
                                               FontFamily="/Resources/#iconfont"
                                               FontSize=" 20">
                                            <Label.Style>
                                                <Style TargetType="Label">
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding IsChecked, ElementName=Window_Switch}" Value="true">
                                                            <Setter Property="Foreground" Value="{DynamicResource 全局_按钮_蓝颜色}" />
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Label.Style>
                                        </Label>
                                    </Grid>
                                </ToggleButton>

                            </Grid>



                        </Grid>
                        <!--  图像窗口信息  -->
                        <!--<Grid Grid.Column=" 2">
                            <StackPanel Margin="5"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Orientation="Horizontal">
                                <TextBlock Text="{Binding Camera_IP_Address, StringFormat=相机IP : {0}}"
                                           Margin="4"
                                           FontFamily="{DynamicResource 全局_文字_数字字体}"
                                           FontSize="16" />
                                <TextBlock Text="{Binding Camera_Resolution, StringFormat=分辨率 : {0}}"
                                           Margin="4"
                                           FontFamily="{DynamicResource 全局_文字_数字字体}"
                                           FontSize="16" />
                                <TextBlock Text="{Binding Camera_FrameRate, StringFormat=帧率 : {0}}"
                                           Margin="4"
                                           FontFamily="{DynamicResource 全局_文字_数字字体}"
                                           FontSize="16" />
                            </StackPanel>
                        </Grid>-->
                    </Grid>
                    <Grid Grid.Row=" 1">
                        <Grid.Style>
                            <Style TargetType="Grid">
                                <Setter Property="Visibility" Value="Hidden" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding IsChecked, ElementName=Live_Window_Button}" Value="True">
                                        <Setter Property="Visibility" Value="Visible" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Grid.Style>
                        <!--  窗口蒙版  -->
                        <!--<Border x:Name="Mask"
                                Background="{DynamicResource 全局_按钮_黑颜色}"
                                CornerRadius="0 0 10 10 " />
                        <Border x:Name="Mask1"
                                Background="{DynamicResource 全局_按钮_黑颜色}"
                                CornerRadius="0 0 0 0 " />-->
                        <!--<ContentControl x:Name="Live_Window" Content="{Binding Live_Window}">
                            <ContentControl.OpacityMask>
                                <VisualBrush Visual="{Binding ElementName=Mask}" />
                            </ContentControl.OpacityMask>
                        </ContentControl>-->
                        <!--  相机图像  -->
                        <Halcon:HSmartWindowControlWPF x:Name="Live_Window"
                                                       HorizontalAlignment="Stretch"
                                                       VerticalAlignment="Stretch"
                                                       HorizontalContentAlignment="Center"
                                                       VerticalContentAlignment="Center"
                                                       HDisableAutoResize="True"
                                                       SnapsToDevicePixels="True">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="Loaded">
                                    <i:InvokeCommandAction Command="{Binding Initialization_Camera_Window_Comm}"
                                                           CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                           PassEventArgsToCommand="True" />
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                            <!--<Halcon:HSmartWindowControlWPF.OpacityMask>
                                <VisualBrush Visual="{Binding ElementName=Mask}" />
                            </Halcon:HSmartWindowControlWPF.OpacityMask>-->
                        </Halcon:HSmartWindowControlWPF>
                    </Grid>
                    <Grid Grid.Row=" 1">
                        <Grid.Style>
                            <Style TargetType="Grid">
                                <Setter Property="Visibility" Value="Hidden" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding IsChecked, ElementName=Results_Window_Button}" Value="True">
                                        <Setter Property="Visibility" Value="Visible" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Grid.Style>
                        <!--  结果窗口  -->
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition Height="auto" />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Border Grid.Row=" 0" Grid.Column=" 0"
                                Grid.RowSpan=" 3"
                                Grid.ColumnSpan=" 3"
                                Background="Transparent" />
                        <!--  Halcon 控件  -->

                        <Grid Grid.Row=" 0" Grid.Column="0">
                            <Grid.Style>
                                <Style TargetType="Grid">
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding IsChecked, ElementName=Window_Switch}" Value="true">
                                            <Setter Property="Grid.RowSpan" Value="3" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Grid.Style>

                            <Halcon:HSmartWindowControlWPF x:Name="Results_Window_1"
                                                           HorizontalAlignment="Stretch"
                                                           VerticalAlignment="Stretch"
                                                           HorizontalContentAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           SnapsToDevicePixels="True">
                                <Halcon:HIconicDisplayObjectWPF IconicObject="{Binding Halcon_Window_Display.Results_Window_1}" />
                                <Halcon:HIconicDisplayObjectWPF HColor="{Binding Halcon_Window_Display.Results_Window_1.SetDisplay.SetColor, UpdateSourceTrigger=PropertyChanged}"
                                                                HDraw="{Binding Halcon_Window_Display.Results_Window_1.SetDisplay.SetDraw, UpdateSourceTrigger=PropertyChanged}"
                                                                IconicObject="{Binding Halcon_Window_Display.Results_Window_1.DisplayRegion}" />
                                <Halcon:HIconicDisplayObjectWPF HColored="3" IconicObject="{Binding Halcon_Window_Display.Results_Window_1.DisplayXLD}" />
                                <i:Interaction.Triggers>
                                    <i:EventTrigger EventName="Loaded">
                                        <i:InvokeCommandAction Command="{Binding Initialization_Camera_Window_Comm}"
                                                               CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                               PassEventArgsToCommand="True" />
                                    </i:EventTrigger>
                                </i:Interaction.Triggers>
                            </Halcon:HSmartWindowControlWPF>

                            <Grid x:Name="G1" VerticalAlignment="Bottom">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="auto" />
                                </Grid.ColumnDefinitions>
                                <Border Grid.Column=" 0"
                                        VerticalAlignment="Bottom"
                                        Background="{DynamicResource 全局_按钮_黑颜色}"
                                        Opacity=" 0.8">
                                    <Border.Style>
                                        <Style TargetType="Border">
                                            <Setter Property="Height" Value=" 25" />
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding IsMouseOver, ElementName=G1}" Value="true">
                                                    <Setter Property="Height" Value="150" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Border.Style>
                                </Border>
                                <StackPanel Grid.Column=" 0"
                                            HorizontalAlignment="Left"
                                            Orientation="Horizontal">
                                    <ListView VerticalAlignment="Bottom"
                                              Background="Transparent"
                                              BorderBrush="Transparent"
                                              BorderThickness="0"
                                              ItemContainerStyle="{DynamicResource Vison_Data_ListBoxItemContainerStyle}"
                                              ItemTemplate="{DynamicResource Result_ListViewItemTemplate}"
                                              ItemsSource="{Binding Find_Results1_Window_Result.Text_Arr_UI}"
                                              ScrollViewer.VerticalScrollBarVisibility="Disabled">
                                        <ListView.Style>
                                            <Style TargetType="ListView">
                                                <Setter Property="Height" Value=" 24" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding IsMouseOver, ElementName=G1}" Value="true">
                                                        <Setter Property="Height" Value=" 150" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </ListView.Style>
                                    </ListView>
                                </StackPanel>
                                <Border Grid.Column=" 1"
                                        Height=" 25"
                                        VerticalAlignment="Bottom"
                                        Opacity=" 0.8">
                                    <Border.Style>
                                        <Style TargetType="Border">
                                            <Setter Property="Background" Value="{DynamicResource 全局_按钮_深绿颜色}" />
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding Find_Results1_Window_Result.FInd_Results}" Value="false">
                                                    <Setter Property="Background" Value="{DynamicResource 全局_按钮_红颜色}" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Border.Style>
                                </Border>
                                <StackPanel Grid.Column=" 1"
                                            Margin="5"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Bottom"
                                            Orientation="Horizontal">
                                    <TextBlock Text="窗口_1   "
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                               FontSize="12"
                                               Foreground="{DynamicResource 全局_按钮_白颜色}" />
                                    <TextBlock VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                               FontSize="12"
                                               Foreground="{DynamicResource 全局_按钮_白颜色}">
                                        <TextBlock.Style>
                                            <Style TargetType="TextBlock">
                                                <Setter Property="Text" Value="结果 : PASS" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding Find_Results1_Window_Result.FInd_Results}" Value="false">
                                                        <Setter Property="Text" Value="结果 : NG" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBlock.Style>
                                    </TextBlock>
                                </StackPanel>
                            </Grid>


                        </Grid>



                        <Grid Grid.Row=" 2" Grid.Column=" 0">
                            <Grid.Style>
                                <Style TargetType="Grid">
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding IsChecked, ElementName=Window_Switch}" Value="true">
                                            <Setter Property="Visibility" Value="Collapsed" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Grid.Style>
                            <Halcon:HSmartWindowControlWPF x:Name="Results_Window_2" Grid.Row=" 2" Grid.Column=" 0"
                                                           HorizontalAlignment="Stretch"
                                                           VerticalAlignment="Stretch"
                                                           HorizontalContentAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           SnapsToDevicePixels="True">
                                <Halcon:HIconicDisplayObjectWPF IconicObject="{Binding Results_Window_2.DisplayImage}" />
                                <Halcon:HIconicDisplayObjectWPF IconicObject="{Binding Results_Window_2.DisplayRegion}" />
                                <Halcon:HIconicDisplayObjectWPF IconicObject="{Binding Results_Window_2.DisplayXLD}" />
                                <i:Interaction.Triggers>
                                    <i:EventTrigger EventName="Loaded">
                                        <i:InvokeCommandAction Command="{Binding Initialization_Camera_Window_Comm}"
                                                               CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                               PassEventArgsToCommand="True" />
                                    </i:EventTrigger>
                                </i:Interaction.Triggers>
                                <!--<Halcon:HSmartWindowControlWPF.OpacityMask>
                                    <VisualBrush Visual="{Binding ElementName=Mask1}" />
                                </Halcon:HSmartWindowControlWPF.OpacityMask>-->
                            </Halcon:HSmartWindowControlWPF>
                            <Grid x:Name="G2" VerticalAlignment="Bottom">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="auto" />
                                </Grid.ColumnDefinitions>
                                <Border Grid.Column=" 0"
                                        VerticalAlignment="Bottom"
                                        Background="{DynamicResource 全局_按钮_黑颜色}"
                                        Opacity=" 0.8">
                                    <Border.Style>
                                        <Style TargetType="Border">
                                            <Setter Property="Height" Value=" 25" />
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding IsMouseOver, ElementName=G2}" Value="true">
                                                    <Setter Property="Height" Value="150" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Border.Style>
                                </Border>
                                <StackPanel Grid.Column=" 0"
                                            HorizontalAlignment="Left"
                                            Orientation="Horizontal">
                                    <ListView VerticalAlignment="Bottom"
                                              Background="Transparent"
                                              BorderBrush="Transparent"
                                              BorderThickness="0"
                                              ItemContainerStyle="{DynamicResource Vison_Data_ListBoxItemContainerStyle}"
                                              ItemTemplate="{DynamicResource Result_ListViewItemTemplate}"
                                              ItemsSource="{Binding Find_Results4_Window_Result.Text_Arr_UI}"
                                              ScrollViewer.VerticalScrollBarVisibility="Disabled">
                                        <ListView.Style>
                                            <Style TargetType="ListView">
                                                <Setter Property="Height" Value=" 24" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding IsMouseOver, ElementName=G2}" Value="true">
                                                        <Setter Property="Height" Value="150" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </ListView.Style>
                                    </ListView>
                                </StackPanel>
                                <Border Grid.Column=" 1"
                                        Height=" 25"
                                        VerticalAlignment="Bottom"
                                        Opacity=" 0.8">
                                    <Border.Style>
                                        <Style TargetType="Border">
                                            <Setter Property="Background" Value="{DynamicResource 全局_按钮_深绿颜色}" />
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding Find_Results4_Window_Result.FInd_Results}" Value="false">
                                                    <Setter Property="Background" Value="{DynamicResource 全局_按钮_红颜色}" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Border.Style>
                                </Border>
                                <StackPanel Grid.Column=" 1"
                                            Margin="5"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Bottom"
                                            Orientation="Horizontal">
                                    <TextBlock Text="窗口_4   "
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                               FontSize="12"
                                               Foreground="{DynamicResource 全局_按钮_白颜色}" />
                                    <TextBlock VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                               FontSize="12"
                                               Foreground="{DynamicResource 全局_按钮_白颜色}">
                                        <TextBlock.Style>
                                            <Style TargetType="TextBlock">
                                                <Setter Property="Text" Value="结果 : PASS" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding Find_Results4_Window_Result.FInd_Results}" Value="false">
                                                        <Setter Property="Text" Value="结果 : NG" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBlock.Style>
                                    </TextBlock>
                                </StackPanel>
                            </Grid>

                        </Grid>


                        <Grid Grid.Row="0" Grid.Column=" 2">
                            <Grid.Style>
                                <Style TargetType="Grid">
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding IsChecked, ElementName=Window_Switch}" Value="true">
                                            <Setter Property="Grid.RowSpan" Value="3" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Grid.Style>
                            <Halcon:HSmartWindowControlWPF x:Name="Results_Window_4" Grid.Row=" 0" Grid.Column=" 2"
                                                           HorizontalAlignment="Stretch"
                                                           VerticalAlignment="Stretch"
                                                           HorizontalContentAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           SnapsToDevicePixels="True">
                                <Halcon:HIconicDisplayObjectWPF IconicObject="{Binding Results_Window_4.DisplayImage}" />
                                <Halcon:HIconicDisplayObjectWPF IconicObject="{Binding Results_Window_4.DisplayRegion}" />
                                <Halcon:HIconicDisplayObjectWPF IconicObject="{Binding Results_Window_4.DisplayXLD}" />
                                <i:Interaction.Triggers>
                                    <i:EventTrigger EventName="Loaded">
                                        <i:InvokeCommandAction Command="{Binding Initialization_Camera_Window_Comm}"
                                                               CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                               PassEventArgsToCommand="True" />
                                    </i:EventTrigger>
                                </i:Interaction.Triggers>
