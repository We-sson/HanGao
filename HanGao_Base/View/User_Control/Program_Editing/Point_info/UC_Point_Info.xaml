                               VerticalAlignment="Center"
                               FontFamily="/Resources/#iconfont"
                               FontSize="20"
                               Foreground="{DynamicResource 全局_按钮_绿颜色}" />
                </Button>
                <TextBlock Text="{Binding New_Set_Data.Welding_Name, FallbackValue=坐标偏移, Mode=OneWay, StringFormat=\{0\} 坐标偏移}"
