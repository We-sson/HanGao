                   Foreground="{DynamicResource 全局_按钮_黑颜色}" />
        <TextBlock Grid.Row="12" Grid.Column="0"
                   Grid.ColumnSpan="2"
                   Text="焊接功率:"
                   Margin="10,0,0,0"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   FontSize="20"
                   Foreground="{DynamicResource 全局_按钮_黑颜色}" />




        <!--  第四列  -->
        <Slider Style="{DynamicResource Vision_SliderStyle}" x:Name="S_CDIS" Grid.Row="8" Grid.Column="3"
                Margin="4"
                VerticalAlignment="Center"
                HorizontalContentAlignment="Stretch"
                VerticalContentAlignment="Stretch"
                IsSnapToTickEnabled="True"
                Maximum="500"
                Minimum="0"
                TickFrequency="1"
                Value="{Binding New_Set_Data.Welding_CDIS}" />

        <Slider Style="{DynamicResource Vision_SliderStyle}" x:Name="S_ACC" Grid.Row="9" Grid.Column="3"
                Margin="4"
                VerticalAlignment="Center"
                HorizontalContentAlignment="Stretch"
                VerticalContentAlignment="Stretch"
                IsSnapToTickEnabled="True"
                Maximum="100"
                Minimum="0"
                TickFrequency="1"
                Value="{Binding New_Set_Data.Welding_ACC}" />

        <Slider Style="{DynamicResource Vision_SliderStyle}" x:Name="S_Speed" Grid.Row="10" Grid.Column="3"
                Margin="4"
                VerticalAlignment="Center"
                HorizontalContentAlignment="Stretch"
                VerticalContentAlignment="Stretch"
                IsSnapToTickEnabled="True"
                Maximum="2"
                Minimum="0"
                TickFrequency="0.001"
                Value="{Binding New_Set_Data.Welding_Speed}" />

        <Slider Style="{DynamicResource Vision_SliderStyle}" x:Name="S_Angle" Grid.Row="11" Grid.Column="3"
                Margin="4"
                VerticalAlignment="Center"
                HorizontalContentAlignment="Stretch"
                VerticalContentAlignment="Stretch"
                IsSnapToTickEnabled="True"
                Maximum="30"
                Minimum="-30"
                TickFrequency="0.1"
                Value="{Binding New_Set_Data.Welding_Angle}" />

        <Slider Style="{DynamicResource Vision_SliderStyle}" x:Name="S_Power" Grid.Row="12" Grid.Column="3"
                Margin="4"
                VerticalAlignment="Center"
                HorizontalContentAlignment="Stretch"
                VerticalContentAlignment="Stretch"
                IsSnapToTickEnabled="True"
                Maximum="100"
                Minimum="0"
                TickFrequency="1"
                Value="{Binding New_Set_Data.Welding_Power}" />

        <!--  第二列  -->

        <NumLib:DoubleUpDown Style="{DynamicResource DoubleUpDown_Style}" Grid.Row="8" Grid.Column="4"
                             Margin="10"
                             VerticalAlignment="Center"
                             VerticalContentAlignment="Center"
                             DisplayLength=" 5"
                             FontSize=" 22"
                             FormatString="F2"
                             LargeStepSize="1"
                             StepSize="1"
                             Tag="mm"
                             Value="{Binding ElementName=S_CDIS, Path=Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
        <!--<TextBox Style="{DynamicResource InPut_TextBoxStyle}" x:Name="CDIS" Grid.Row="8" Grid.Column="4"
                 Text="{Binding ElementName=S_CDIS, Path=Value}"
                 Margin="10"
                 Tag="mm" />-->

        <NumLib:DoubleUpDown Style="{DynamicResource DoubleUpDown_Style}" Grid.Row="9" Grid.Column="4"
                             Margin="10"
                             VerticalAlignment="Center"
                             VerticalContentAlignment="Center"
                             DisplayLength=" 5"
                             FontSize=" 22"
                             FormatString="F2"
                             LargeStepSize="1"
                             StepSize="1"
                             Tag="%"
                             Value="{Binding ElementName=S_ACC, Path=Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

        <!--<TextBox Style="{DynamicResource InPut_TextBoxStyle}" x:Name="ACC" Grid.Row="9" Grid.Column="4"
                 Text="{Binding ElementName=S_ACC, Path=Value}"
                 Margin="10"
                 Tag="%" />-->


        <NumLib:DoubleUpDown Style="{DynamicResource DoubleUpDown_Style}" Grid.Row="10" Grid.Column="4"
                             Margin="10"
                             VerticalAlignment="Center"
                             VerticalContentAlignment="Center"
                             DisplayLength="5"
                             FontSize=" 22"
                             FormatString="F2"
                             LargeStepSize="0.01"
                             StepSize="0.1"
                             Tag="m/s"
                             Value="{Binding ElementName=S_Speed, Path=Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

        <!--<TextBox Style="{DynamicResource InPut_TextBoxStyle}" x:Name="Speed" Grid.Row="10" Grid.Column="4"
                 Text="{Binding ElementName=S_Speed, Path=Value}"
                 Margin="10"
                 Tag="m/s" />-->

        <NumLib:DoubleUpDown Style="{DynamicResource DoubleUpDown_Style}" Grid.Row="11" Grid.Column="4"
                             Margin="10"
                             VerticalAlignment="Center"
                             VerticalContentAlignment="Center"
                             DisplayLength=" 5"
                             FontSize=" 22"
                             FormatString="F2"
                             LargeStepSize="0.1"
                             StepSize="0.1"
                             Tag="°"
                             Value="{Binding ElementName=S_Angle, Path=Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />


        <!--<TextBox Style="{DynamicResource InPut_TextBoxStyle}" x:Name="Angle" Grid.Row="11" Grid.Column="4"
                 Text="{Binding ElementName=S_Angle, Path=Value}"
                 Margin="10"
                 Tag="°" />-->


        <NumLib:DoubleUpDown Style="{DynamicResource DoubleUpDown_Style}" Grid.Row="12" Grid.Column="4"
                             Margin="10"
                             VerticalAlignment="Center"
                             VerticalContentAlignment="Center"
                             DisplayLength=" 5"
                             FontSize=" 22"
                             FormatString="F2"
                             LargeStepSize="1"
                             StepSize="1"
                             Tag="%"
                             Value="{Binding ElementName=S_Power, Path=Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />


        <!--<TextBox Style="{DynamicResource InPut_TextBoxStyle}" x:Name="Power" Grid.Row="12" Grid.Column="4"
                 Text="{Binding ElementName=S_Power, Path=Value}"
                 Margin="10"
                 Tag="%" />-->



        <!--  功能按钮  -->
        <Button Style="{DynamicResource ButtonStyle3}" Grid.Row="11" Grid.Column="5"
                Grid.ColumnSpan="1"
                Content="保存"
                Width=" 120"
                Height=" 50"
                Margin="50,0,49,0"
                VerticalAlignment="Center"
                Background="{DynamicResource 全局_按钮_绿颜色}"
                Effect="{DynamicResource 全局背景阴影}"
                FontSize="24">
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="Click">
                    <i:InvokeCommandAction Command="{Binding Craft_Point_Info_SaveToXml_Comm}"
                                           CommandParameter="{Binding Mode=OneWay, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type HanGao:MainWindow}}}"
                                           PassEventArgsToCommand="True" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Button>
        <Button Style="{DynamicResource ButtonStyle3}" Grid.Row="12" Grid.Column="5"
                Grid.ColumnSpan="1"
                Content="取消"
                Width=" 120"
                Height=" 50"
                Margin="50,0,49,0"
                VerticalAlignment="Center"
                Background="{DynamicResource 全局_按钮_红颜色}"
                Effect="{DynamicResource 全局背景阴影}"
                FontSize="24">
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="Click">
                    <i:InvokeCommandAction Command="{Binding Craft_Point_Info_Close_Comm}"
                                           CommandParameter="{Binding Mode=OneWay, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type HanGao:MainWindow}}}"
                                           PassEventArgsToCommand="True" />
                </i:EventTrigger>
            </i:Interaction.Triggers>
        </Button>




    </Grid>
</UserControl>
