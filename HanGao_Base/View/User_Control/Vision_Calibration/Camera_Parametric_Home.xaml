<Window x:Class="HanGao.View.User_Control.Vision_Calibration.Camera_Parametric_Home"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Enum_Converter="clr-namespace:Halcon_SDK_DLL.WPF_Converter;assembly=Halcon_SDK_DLL"
        xmlns:Halcon="http://schemas.mvtec.com/halcondotnet"
        xmlns:Halcon_DLL="clr-namespace:Halcon_SDK_DLL;assembly=Halcon_SDK_DLL"
        xmlns:Halcon_Enum="clr-namespace:Halcon_SDK_DLL.Model;assembly=Halcon_SDK_DLL"
        xmlns:MVS="clr-namespace:MvCamCtrl.NET.CameraParams;assembly=MvCamCtrl.Net"
        xmlns:MVS_Enum="clr-namespace:MVS_SDK_Base.Model;assembly=MVS_SDK"
        xmlns:Openfile="clr-namespace:HanGao.View.User_Control.OpenFile"
        xmlns:UC_Working="clr-namespace:HanGao.View.User_Control"
        xmlns:VTK="clr-namespace:Kitware.VTK;assembly=Kitware.VTK"
        xmlns:Vision_control="clr-namespace:HanGao.View.User_Control.Vision_Control"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
        xmlns:local="clr-namespace:HanGao.View.User_Control.Vision_Calibration"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        Title="相机内参标定工具"
        Width="1400"
        Height="1200"
        d:DataContext="{Binding Source={StaticResource Locator}, Path=Vision_Calibration_Home_VM}"
        WindowStyle="SingleBorderWindow"
        mc:Ignorable="d">

    <Window.Resources>
        <Enum_Converter:Halcon_StringConverter x:Key="Halcon_StringConverter" />
        <Enum_Converter:Halcon_EnumConverter x:Key="Halcon_EnumConverter" />
        <Enum_Converter:Radio_CheckedToEnumConverter x:Key="Radio_CheckedToEnum" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
        <Enum_Converter:EnumDescriptionConverter x:Key="Enum_DescriptionConverter" />


        <ObjectDataProvider x:Key="Halocn_Camera_Calibration"
                            MethodName="GetValues"
                            ObjectType="{x:Type sys:Enum}">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="Halcon_Enum:Halocn_Camera_Calibration_Enum" />
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>

        <ObjectDataProvider x:Key="Halcon_Optimization_Enum"
                            MethodName="GetValues"
                            ObjectType="{x:Type sys:Enum}">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="Halcon_DLL:HandEye_Optimization_Method_Enum" />
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>


    </Window.Resources>
    <i:Interaction.Triggers>
        <i:EventTrigger EventName="Loaded">
            <i:InvokeCommandAction Command="{Binding Initialization_Halcon_Window_Comm}"
                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                   PassEventArgsToCommand="True" />
        </i:EventTrigger>
        <i:EventTrigger EventName="Closed">
            <i:InvokeCommandAction Command="{Binding Closed_Window_Dsipos_Comm}"
                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                   PassEventArgsToCommand="True" />
        </i:EventTrigger>
    </i:Interaction.Triggers>
    <Grid Background="{DynamicResource 全局_背景_白颜色}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="9*" />
            <ColumnDefinition Width="59*" />
            <ColumnDefinition Width=" 420" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.8*" />
            <RowDefinition Height=" 0.35*" />
        </Grid.RowDefinitions>


        <TextBlock Grid.Row=" 0" Grid.Column=" 0"
                   Grid.ColumnSpan=" 3"
                   Text="&#xe640;"
                   Margin="15,10,0,0"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   FontFamily="/Resources/#iconfont"
                   FontSize="30"
                   Foreground="{DynamicResource 全局_按钮_蓝颜色}" />



        <TabControl Style="{DynamicResource TabControl_Style}" x:Name="Tab_Window" Grid.Row="0" Grid.Column=" 0"
                    Grid.ColumnSpan="2"
                    Margin="5,5,5,5"
                    Padding="0"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Background="Transparent"
                    BorderBrush="{DynamicResource 全局_按钮_蓝颜色}"
                    BorderThickness="3"
                    SelectedIndex="0"
                    TabStripPlacement="Top">
            <!--<i:Interaction.Triggers>
                <i:EventTrigger EventName="Loaded">
                    <i:InvokeCommandAction Command="{Binding Initialization_Halcon_UserControl_Comm}"
                                           CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                           PassEventArgsToCommand="True" />
                </i:EventTrigger>
            </i:Interaction.Triggers>-->

            <TabItem Style="{DynamicResource Vision_TabItemStyle}"
                     Padding="50,5,50,5"
                     FontSize=" 22"
                     Header="相机窗口">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto" />
                        <RowDefinition />
                    </Grid.RowDefinitions>

                    <Border Grid.Row=" 0" Grid.Column=" 0"
                            Grid.ColumnSpan=" 3"
                            Height=" 40"
                            Padding="3"
                            Background="{DynamicResource 全局_背景_白颜色}">
                        <StackPanel Orientation="Horizontal">


                            <ToggleButton Style="{DynamicResource Window_ToggleButtonStyle}" x:Name="Calibration_Switch"
                                          IsChecked="False"
                                          Margin="4,0,4,0"
                                          Padding="0"
                                          BorderThickness="2">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <Label Grid.Column=" 0"
                                           Content="&#xe627;"
                                           Margin="2"
                                           Padding="15,0,15,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontFamily="/Resources/#iconfont"
                                           FontSize=" 20">
                                        <Label.Style>
                                            <Style TargetType="Label">
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding IsChecked, ElementName=Calibration_Switch}" Value="false">
                                                        <Setter Property="Foreground" Value="{DynamicResource 全局_按钮_蓝颜色}" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Label.Style>
                                    </Label>

                                    <Label Grid.Column=" 1"
                                           Content="&#xe62d;"
                                           Margin="2"
                                           Padding="15,0,15,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontFamily="/Resources/#iconfont"
                                           FontSize=" 20">
                                        <Label.Style>
                                            <Style TargetType="Label">
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding IsChecked, ElementName=Calibration_Switch}" Value="true">
                                                        <Setter Property="Foreground" Value="{DynamicResource 全局_按钮_蓝颜色}" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Label.Style>
                                    </Label>
                                </Grid>
                            </ToggleButton>

                            <RadioButton Style="{DynamicResource Halcon_Window_RadioButton_Style}"
                                         IsChecked="{Binding Halcon_Camera_Calibra.ShowMaxGray}"
                                         Margin="0,0,0,0"
                                         Padding="0,0,0,0"
                                         HorizontalAlignment="Center"
                                         VerticalAlignment="Center"
                                         FontSize=" 16"
                                         GroupName="DisplayShow">
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="&#xe636;"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontFamily="/Resources/#iconfont"
                                           FontSize=" 20"
                                           Foreground="{DynamicResource 全局_按钮_蓝颜色}" />
                                    <Label Content="过曝显示"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontSize=" 14" />
                                </StackPanel>
                            </RadioButton>

                            <RadioButton Style="{DynamicResource Halcon_Window_RadioButton_Style}"
                                         IsChecked="{Binding Halcon_Camera_Calibra.ShowMinGray}"
                                         Margin="0,0,0,0"
                                         HorizontalAlignment="Center"
                                         VerticalAlignment="Center"
                                         FontSize=" 16"
                                         GroupName="DisplayShow">
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="&#xe636;"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontFamily="/Resources/#iconfont"
                                           FontSize=" 20"
                                           Foreground="{DynamicResource 全局_按钮_蓝颜色}" />
                                    <Label Content="过暗显示"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontSize=" 14" />
                                </StackPanel>
                            </RadioButton>

                            <RadioButton Style="{DynamicResource Halcon_Window_RadioButton_Style}"
                                         IsChecked="{Binding Halcon_Camera_Calibra.ShowHObject}"
                                         Margin="0,0,0,0"
                                         Padding="0,0,0,0"
                                         HorizontalAlignment="Center"
                                         VerticalAlignment="Center"
                                         FontSize=" 16"
                                         GroupName="DisplayShow">
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="&#xe637;"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontFamily="/Resources/#iconfont"
                                           FontSize=" 20"
                                           Foreground="{DynamicResource 全局_按钮_蓝颜色}" />
                                    <Label Content="轮廓显示"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontSize=" 14" />
                                </StackPanel>
                            </RadioButton>

                        </StackPanel>
                    </Border>

                    <Grid Grid.Row=" 1" Grid.Column="0"
                          Grid.ColumnSpan=" 3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width=" *" />

                            <ColumnDefinition Width=" auto" />

                            <ColumnDefinition>
                                <ColumnDefinition.Style>
                                    <Style TargetType="ColumnDefinition">
                                        <Setter Property="Width" Value="1*" />
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding IsChecked, ElementName=Calibration_Switch}" Value="true">
                                                <Setter Property="Width" Value="auto" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </ColumnDefinition.Style>
                            </ColumnDefinition>
                        </Grid.ColumnDefinitions>


                        <Grid Grid.Column=" 0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height=" auto" />
                                <RowDefinition Height=" *" />
                            </Grid.RowDefinitions>


                            <Border Grid.Row="0"
                                    Height=" 35"
                                    Margin="0"
                                    Background="{DynamicResource 全局_按钮_蓝颜色}"
                                    CornerRadius=" 0">




                                <Grid>

                                    <TextBlock Text="{Binding Camera_0_Select_Val.Camera_Info.SerialNumber, FallbackValue=..., StringFormat={}Camera: {0}}"
                                               TextWrapping="Wrap"
                                               Margin="0,0,0,0"
                                               Padding="20,0,0,0"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                               FontSize="16 "
                                               Foreground="{DynamicResource 全局_背景_白颜色}"
                                               IsEnabled="True"
                                               TextTrimming="CharacterEllipsis" />

                                    <TextBlock Text="{Binding Camera_0_Select_Val.Camera_Check_Delay, StringFormat={}检测延时:{0}ms, FallbackValue=检测延时:0}"
                                               TextWrapping="Wrap"
                                               Margin="0,0,0,0"
                                               Padding="0,0,10,0"
                                               HorizontalAlignment="Right"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_中文字体}"
                                               FontSize="16 "
                                               Foreground="{DynamicResource 全局_按钮_黄颜色}"
                                               IsEnabled="True"
                                               TextTrimming="CharacterEllipsis" />
                                </Grid>

                            </Border>


                            <Halcon:HSmartWindowControlWPF x:Name="Calibration_Window_1" Grid.Row=" 1"
                                                           MinWidth="400"
                                                           MinHeight="400"
                                                           HorizontalAlignment="Stretch"
                                                           VerticalAlignment="Stretch"
                                                           HorizontalContentAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           HDisableAutoResize="True"
                                                           SnapsToDevicePixels="True">
                                <Halcon:HIconicDisplayObjectWPF IconicObject="{Binding Halcon_Window_Display.Calibration_Window_1.DisplayImage}" />
                                <Halcon:HIconicDisplayObjectWPF HColor="{Binding Halcon_Window_Display.Calibration_Window_1.SetDisplay.SetColor, UpdateSourceTrigger=PropertyChanged}"
                                                                HDraw="{Binding Halcon_Window_Display.Calibration_Window_1.SetDisplay.SetDraw, UpdateSourceTrigger=PropertyChanged}"
                                                                IconicObject="{Binding Halcon_Window_Display.Calibration_Window_1.DisplayRegion}" />
                                <Halcon:HIconicDisplayObjectWPF HColored="3" IconicObject="{Binding Halcon_Window_Display.Calibration_Window_1.DisplayXLD}" />
                            </Halcon:HSmartWindowControlWPF>


                        </Grid>


                        <Border Grid.Column=" 1"
                                Width=" 2"
                                HorizontalAlignment="Stretch"
                                Background="{DynamicResource 全局_按钮_深灰颜色}">
                            <Border.Style>
                                <Style TargetType="Border">
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding IsChecked, ElementName=Calibration_Switch}" Value="true">
                                            <Setter Property="Visibility" Value="Collapsed" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Border.Style>
                        </Border>


                        <Grid Grid.Column=" 2">
                            <Grid.RowDefinitions>
                                <RowDefinition Height=" auto" />
                                <RowDefinition Height=" *" />
                            </Grid.RowDefinitions>


                            <Border Grid.Row="0"
                                    Height=" 35"
                                    Margin="0"
                                    Background="{DynamicResource 全局_按钮_蓝颜色}"
                                    CornerRadius=" 0">




                                <Grid>

                                    <TextBlock Text="{Binding Camera_1_Select_Val.Camera_Info.SerialNumber, FallbackValue=..., StringFormat={}Camera: {0}}"
                                               TextWrapping="Wrap"
                                               Margin="0,0,0,0"
                                               Padding="20,0,0,0"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                               FontSize="16 "
                                               Foreground="{DynamicResource 全局_背景_白颜色}"
                                               IsEnabled="True"
                                               TextTrimming="CharacterEllipsis" />

                                    <TextBlock Text="{Binding Camera_1_Select_Val.Camera_Check_Delay, StringFormat={}检测延时:{0}ms, FallbackValue=检测延时:0}"
                                               TextWrapping="Wrap"
                                               Margin="0,0,0,0"
                                               Padding="0,0,10,0"
                                               HorizontalAlignment="Right"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_中文字体}"
                                               FontSize="16 "
                                               Foreground="{DynamicResource 全局_按钮_黄颜色}"
                                               IsEnabled="True"
                                               TextTrimming="CharacterEllipsis" />
                                </Grid>

                            </Border>

                            <Halcon:HSmartWindowControlWPF x:Name="Calibration_Window_2" Grid.Row=" 1"
                                                           MinWidth="400"
                                                           MinHeight="400"
                                                           HorizontalAlignment="Stretch"
                                                           VerticalAlignment="Stretch"
                                                           HorizontalContentAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           HDisableAutoResize="True"
                                                           SnapsToDevicePixels="True">
                                <Halcon:HIconicDisplayObjectWPF IconicObject="{Binding Halcon_Window_Display.Calibration_Window_2.DisplayImage}" />
                                <Halcon:HIconicDisplayObjectWPF HColor="{Binding Halcon_Window_Display.Calibration_Window_2.SetDisplay.SetColor, UpdateSourceTrigger=PropertyChanged}"
                                                                HDraw="{Binding Halcon_Window_Display.Calibration_Window_2.SetDisplay.SetDraw, UpdateSourceTrigger=PropertyChanged}"
                                                                IconicObject="{Binding Halcon_Window_Display.Calibration_Window_2.DisplayRegion}" />
                                <Halcon:HIconicDisplayObjectWPF HColored="3" IconicObject="{Binding Halcon_Window_Display.Calibration_Window_2.DisplayXLD}" />
                                <Halcon:HSmartWindowControlWPF.Style>
                                    <Style TargetType="Halcon:HSmartWindowControlWPF">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding IsChecked, ElementName=Calibration_Switch}" Value="true">
                                                <Setter Property="Visibility" Value="Collapsed" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Halcon:HSmartWindowControlWPF.Style>

                            </Halcon:HSmartWindowControlWPF>


                        </Grid>



                    </Grid>




                </Grid>
            </TabItem>
            <TabItem Style="{DynamicResource Vision_TabItemStyle}"
                     Padding="50,5,50,5"
                     FontSize=" 22"
                     Header="三维对象图">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />

                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto" />
                        <RowDefinition />
                        <RowDefinition Height=" auto" />
                    </Grid.RowDefinitions>

                    <Border Grid.Row=" 0" Grid.Column=" 0"
                            Grid.ColumnSpan=" 3"
                            Height=" 40"
                            Padding="3"
                            Background="{DynamicResource 全局_背景_白颜色}">
                        <StackPanel Orientation="Horizontal">


                            <StackPanel Margin="4,0,4,0" Orientation="Horizontal">

                                <TextBlock Text="显示方式"
                                           Margin="4,0,4,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontFamily="{DynamicResource 全局_文字_中文字体}"
                                           FontSize="16"
                                           Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                <ComboBox Style="{DynamicResource Data_ComboBoxStyle}" x:Name="ShapeModel_UI" Grid.Column=" 1"
                                          Width=" 80"
                                          HorizontalContentAlignment="Center"
                                          VerticalContentAlignment="Center"
                                          BorderBrush="{DynamicResource 全局_按钮_黑颜色}"
                                          BorderThickness="2"
                                          IsSynchronizedWithCurrentItem="True"
                                          SelectedIndex="{Binding HDisplay_3D.Scene3D_Instance.Attribute, ConverterParameter={x:Type Halcon_Enum:Attribute_Val_Enum}, Converter={StaticResource Halcon_EnumConverter}}">
                                    <ComboBoxItem>
                                        <TextBlock Text="自动"
                                                   TextWrapping="Wrap"
                                                   Padding="10,5,10,5"
                                                   FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                   FontSize="14"
                                                   Foreground="{DynamicResource 全局_按钮_黑颜色}"
                                                   TextTrimming="CharacterEllipsis" />
                                    </ComboBoxItem>
                                    <ComboBoxItem>

                                        <TextBlock Text="面"
                                                   TextWrapping="Wrap"
                                                   Padding="10,5,10,5"
                                                   FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                   FontSize="14"
                                                   Foreground="{DynamicResource 全局_按钮_黑颜色}"
                                                   TextTrimming="CharacterEllipsis" />
                                    </ComboBoxItem>
                                    <ComboBoxItem>

                                        <TextBlock Text="原始"
                                                   TextWrapping="Wrap"
                                                   Padding="10,5,10,5"
                                                   FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                   FontSize="14"
                                                   Foreground="{DynamicResource 全局_按钮_黑颜色}"
                                                   TextTrimming="CharacterEllipsis" />
                                    </ComboBoxItem>
                                    <ComboBoxItem>

                                        <TextBlock Text="点云"
                                                   TextWrapping="Wrap"
                                                   Padding="10,5,10,5"
                                                   FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                   FontSize="14"
                                                   Foreground="{DynamicResource 全局_按钮_黑颜色}"
                                                   TextTrimming="CharacterEllipsis" />
                                    </ComboBoxItem>
                                    <ComboBoxItem>

                                        <TextBlock Style="{DynamicResource ComboBox_TextBlok_Style}"
                                                   Text="线段"
                                                   TextWrapping="Wrap"
                                                   Padding="10,5,10,5"
                                                   FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                   FontSize="14"
                                                   Foreground="{DynamicResource 全局_按钮_黑颜色}"
                                                   TextTrimming="CharacterEllipsis" />
                                    </ComboBoxItem>
                                </ComboBox>


                                <TextBlock Text="透明度"
                                           Margin="4,0,4,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontFamily="{DynamicResource 全局_文字_中文字体}"
                                           FontSize="16"
                                           Foreground="{DynamicResource 全局_按钮_黑颜色}" />

                                <Slider Style="{DynamicResource Vision_SliderStyle}" x:Name="Scene3D_Alpha" Grid.Column=" 1"
                                        Width=" 50"
                                        Margin="5"
                                        VerticalAlignment="Center"
                                        HorizontalContentAlignment="Stretch"
                                        VerticalContentAlignment="Stretch"
                                        IsSnapToTickEnabled="True"
                                        Maximum="1"
                                        Minimum="0.01"
                                        TickFrequency=" 0.01"
                                        Value="{Binding HDisplay_3D.Scene3D_Param.Alpha, UpdateSourceTrigger=PropertyChanged}" />

                                <TextBox Grid.Column=" 2"
                                         Text="{Binding ElementName=Scene3D_Alpha, Path=Value, StringFormat={}{0:F2}}"
                                         Width="80"
                                         HorizontalContentAlignment="Center"
                                         VerticalContentAlignment="Center"
                                         Background="Transparent"
                                         BorderBrush="{DynamicResource 全局_按钮_黑颜色}"
                                         BorderThickness="2"
                                         FontSize="16" />
                            </StackPanel>

                            <ToggleButton Style="{DynamicResource Function_ToggleButtonStyle}"
                                          IsChecked="{Binding HDisplay_3D.Scene3D_Instance.Disp_pose}"
                                          Margin="4,0,4,0"
                                          Padding="0,0,0,0"
                                          FontSize=" 16">
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="&#xe62f;"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontFamily="/Resources/#iconfont"
                                           FontSize="18"
                                           Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                    <Label Content="坐标系"
                                           FontSize="16"
                                           Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                </StackPanel>

                                <!--<i:Interaction.Triggers>
                                    <i:EventTrigger EventName="Checked">
                                        <i:InvokeCommandAction Command="{Binding Set_Scene3D_Disp_Pose_Comm}" PassEventArgsToCommand="True" />
                                    </i:EventTrigger>
                                </i:Interaction.Triggers>-->
                            </ToggleButton>

                            <ToggleButton Style="{DynamicResource Function_ToggleButtonStyle}"
                                          IsChecked="{Binding HDisplay_3D.Scene3D_Instance.Disp_normals}"
                                          Margin="4,0,4,0"
                                          Padding="0,0,0,0"
                                          FontSize=" 16">
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="&#xe63b;"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontFamily="/Resources/#iconfont"
                                           FontSize=" 18"
                                           Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                    <Label Content="模型法线"
                                           FontSize="16"
                                           Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                </StackPanel>
                            </ToggleButton>
                            <ToggleButton Style="{DynamicResource Function_ToggleButtonStyle}"
                                          IsChecked="{Binding HDisplay_3D.Scene3D_Instance.Disp_lines}"
                                          Margin="4,0,4,0"
                                          Padding="0,0,0,0"
                                          FontSize=" 16">
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="&#xe63d;"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontFamily="/Resources/#iconfont"
                                           FontSize=" 18"
                                           Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                    <Label Content="模型轮廓"
                                           FontSize="16"
                                           Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                </StackPanel>
                            </ToggleButton>


                            <ToggleButton Style="{DynamicResource Function_ToggleButtonStyle}"
                                          IsChecked="False"
                                          Margin="4,0,4,0"
                                          Padding="0,0,0,0"
                                          FontSize=" 16">
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="&#xe629;"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontFamily="/Resources/#iconfont"
                                           FontSize=" 18"
                                           Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                    <Label Content="模型编号"
                                           FontSize="16"
                                           Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                </StackPanel>
                            </ToggleButton>

                        </StackPanel>
                    </Border>







                    <Halcon:HSmartWindowControlWPF x:Name="Calibration_3D_Results" Grid.Row=" 1" Grid.Column=" 0"
                                                   Grid.ColumnSpan=" 2"
                                                   MinWidth="400"
                                                   MinHeight="400"
                                                   HorizontalAlignment="Stretch"
                                                   VerticalAlignment="Stretch"
                                                   HorizontalContentAlignment="Center"
                                                   VerticalContentAlignment="Center"
                                                   HDisableAutoResize="False"
                                                   HDoubleClickToFitContent="False"
                                                   HKeepAspectRatio="False"
                                                   HMoveContent="False"
