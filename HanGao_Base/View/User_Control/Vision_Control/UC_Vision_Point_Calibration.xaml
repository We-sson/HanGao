                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding ElementName=Filtering_Model_UI, Path=SelectedIndex}" Value="2">
                                                <Setter Property="Visibility" Value="Visible" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Grid.Style>



                                <Grid.ColumnDefinitions>
