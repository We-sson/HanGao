                                        IsSnapToTickEnabled="True"
                                        Maximum="50000"
                                        Minimum="0"
                                        SmallChange="10"
                                        Value="{Binding Calibration_Camera_Results.Maximum_Translational}">
                                    <i:Interaction.Triggers>
