                                <Slider Style="{DynamicResource Vision_SliderStyle}" x:Name="Calibration_Camera_0_Error_UI" Grid.Column=" 1"
                                        Margin="5"
                                        VerticalAlignment="Center"
                                        HorizontalContentAlignment="Stretch"
                                        VerticalContentAlignment="Stretch"
                                        IsEnabled="False"
                                        IsSnapToTickEnabled="True"
                                        Maximum="50000"
                                        Minimum="0"
                                        SmallChange="10"
                                        Value="{Binding All_Camera_Results.Camera_0_Results.Result_Error_Val}">
