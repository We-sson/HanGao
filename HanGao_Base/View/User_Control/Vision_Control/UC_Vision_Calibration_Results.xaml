                                          VerticalContentAlignment="Center"
                                          BorderBrush="{DynamicResource 全局_按钮_黑颜色}"
                                          BorderThickness="2"
                                          FontSize="20"
                                          IsDropDownOpen="False"
                                          IsEnabled="False"
                                          IsSynchronizedWithCurrentItem="True"
                                          SelectedIndex="{Binding All_Camera_Results.Camera_0_Results.Camera_Result_Pama.Camera_Calibration_Model, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource Halcon_EnumConverter}, ConverterParameter={x:Type Halcon_Enum:Halocn_Camera_Calibration_Enum}}">

                                    <ComboBoxItem>

                                        <TextBlock Text="面扫描(普通式)"
