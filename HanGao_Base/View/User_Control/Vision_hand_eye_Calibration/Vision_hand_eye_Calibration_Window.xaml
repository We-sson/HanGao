<Window x:Class="HanGao.View.User_Control.Vision_hand_eye_Calibration.Vision_hand_eye_Calibration_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Converts_Enum="clr-namespace:Roboto_Socket_Library;assembly=Roboto_Socket_Library"
        xmlns:EnumModel="clr-namespace:Halcon_SDK_DLL;assembly=Halcon_SDK_DLL"
        xmlns:Enum_Converter="clr-namespace:Halcon_SDK_DLL.WPF_Converter;assembly=Halcon_SDK_DLL"
        xmlns:Halcon="http://schemas.mvtec.com/halcondotnet"
        xmlns:Halcon_Enum="clr-namespace:Halcon_SDK_DLL.Model;assembly=Halcon_SDK_DLL"
        xmlns:MVS="clr-namespace:MvCamCtrl.NET.CameraParams;assembly=MvCamCtrl.Net"
        xmlns:MVS_Enum="clr-namespace:MVS_SDK_Base.Model;assembly=MVS_SDK"
        xmlns:Openfile="clr-namespace:HanGao.View.User_Control.OpenFile"
        xmlns:Process_Enum="clr-namespace:Halcon_SDK_DLL.Halcon_Method;assembly=Halcon_SDK_DLL"
        xmlns:Robot="clr-namespace:Roboto_Socket_Library.Model;assembly=Roboto_Socket_Library"
        xmlns:UC_Working="clr-namespace:HanGao.View.User_Control"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
        xmlns:local="clr-namespace:HanGao.View.User_Control.Vision_hand_eye_Calibration"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        Title="视觉手眼标定工具"
        MinWidth=" 1350"
        d:DataContext="{Binding Source={StaticResource Locator}, Path=Vision_hand_eye_Calibration_VM}"
        d:Height="1080"
        d:Width="1920"
        Background="{DynamicResource 全局_背景_白颜色}"
        mc:Ignorable="d">
    <Window.Resources>
        <Enum_Converter:Halcon_EnumConverter x:Key="Halcon_EnumConverter" />
        <Enum_Converter:Radio_CheckedToEnumConverter x:Key="Radio_CheckedToEnum" />
        <Enum_Converter:EnumDescriptionConverter x:Key="Enum_DescriptionConverter" />
        <ObjectDataProvider x:Key="Robot_Enum"
                            MethodName="GetValues"
                            ObjectType="{x:Type sys:Enum}">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="Robot:Socket_Robot_Protocols_Enum" />
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
        <ObjectDataProvider x:Key="Calib_Robot_Enum"
                            MethodName="GetValues"
                            ObjectType="{x:Type sys:Enum}">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="Halcon_Enum:Robot_Type_Enum" />
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>

        <ObjectDataProvider x:Key="Pose_Enum"
                            MethodName="GetValues"
                            ObjectType="{x:Type sys:Enum}">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="Halcon_Enum:Halcon_Pose_Type_Enum" />
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>

        <ObjectDataProvider x:Key="Halocn_Camera_Calibration"
                            MethodName="GetValues"
                            ObjectType="{x:Type sys:Enum}">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="Halcon_Enum:Halocn_Camera_Calibration_Enum" />
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>

        <ObjectDataProvider x:Key="Halcon_Optimization_Enum"
                            MethodName="GetValues"
                            ObjectType="{x:Type sys:Enum}">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="EnumModel:HandEye_Optimization_Method_Enum" />
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
    </Window.Resources>

    <i:Interaction.Triggers>
        <!--<i:EventTrigger EventName="Loaded">
            <i:InvokeCommandAction Command="{Binding Initialization_HandEye_Window_Comm}"
                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                   PassEventArgsToCommand="True" />
        </i:EventTrigger>-->

        <i:EventTrigger EventName="Closed">
            <i:InvokeCommandAction Command="{Binding Closed_HandEye_Window_Dsipos_Comm}"
                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                   PassEventArgsToCommand="True" />
        </i:EventTrigger>
    </i:Interaction.Triggers>

    <Grid ShowGridLines="False">
        <Grid.RowDefinitions>
            <RowDefinition Height="55" />
            <RowDefinition Height="3" />
            <RowDefinition />
        </Grid.RowDefinitions>


        <Border Grid.Row=" 0" Grid.Column=" 0"
                Background="{DynamicResource 全局_按钮_浅棕色颜色}" />




        <TextBlock Grid.Row="0"
                   Text="&#xe640;"
                   Margin="15,10,0,0"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   FontFamily="/Resources/#iconfont"
                   FontSize="30"
                   Foreground="{DynamicResource 全局_按钮_蓝颜色}" />



        <Grid Grid.Column="1"
              HorizontalAlignment="Center"
              VerticalAlignment="Stretch">


            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />

            </Grid.ColumnDefinitions>


            <RadioButton Style="{StaticResource 标题_选项_样式}" x:Name="Home_Camera_Para" Grid.Column="0"
                         IsChecked="True"
                         Margin="20,0,20,0"
                         HorizontalAlignment="Center"
                         HorizontalContentAlignment="Center"
                         VerticalContentAlignment="Center"
                         FontSize="14"
                         GroupName="HandEye_Home">





                <StackPanel Orientation="Horizontal">

                    <Image Height=" 35"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center"
                           Effect="{DynamicResource 全局背景阴影}"
                           Source="/Resources/Vision_handEye_3.png" />
                    <TextBlock Text="标定参数"
                               Margin="20,0,0,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontFamily="{DynamicResource 全局_文字_中文字体}"
                               FontSize="24"
                               Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                </StackPanel>



            </RadioButton>


            <RadioButton Style="{StaticResource 标题_选项_样式}" x:Name="Home_Camera_Setting" Grid.Column="1"
                         Margin="20,0,20,0"
                         HorizontalAlignment="Center"
                         VerticalContentAlignment="Center"
                         GroupName="HandEye_Home">

                <StackPanel Orientation="Horizontal">

                    <Image Height=" 35"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center"
                           Effect="{DynamicResource 全局背景阴影}"
                           Source="/Resources/Vision_handEye_9.png" />
                    <TextBlock Text="相机配置"
                               Margin="20,0,0,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontFamily="{DynamicResource 全局_文字_中文字体}"
                               FontSize="24"
                               Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                </StackPanel>

            </RadioButton>

            <RadioButton Style="{StaticResource 标题_选项_样式}" x:Name="Home_Camera_GetImage" Grid.Column="2"
                         Margin="20,0,20,0"
                         HorizontalAlignment="Stretch"
                         VerticalAlignment="Stretch"
                         HorizontalContentAlignment="Stretch"
                         VerticalContentAlignment="Center"
                         GroupName="HandEye_Home">
                <!--<i:Interaction.Triggers>
                        <i:EventTrigger EventName="Checked">
                            <i:InvokeCommandAction Command="{Binding Set_User_Show}" CommandParameter="{Binding RelativeSource={RelativeSource AncestorType={x:Type RadioButton}, AncestorLevel=1, Mode=FindAncestor}}" />
                        </i:EventTrigger>
                    </i:Interaction.Triggers>-->


                <StackPanel Orientation="Horizontal">

                    <Image Height=" 35"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center"
                           Effect="{DynamicResource 全局背景阴影}"
                           Source="/Resources/Vision_handEye_6.png" />
                    <TextBlock Text="图像重建"
                               Margin="20,0,0,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontFamily="{DynamicResource 全局_文字_中文字体}"
                               FontSize="24"
                               Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                </StackPanel>


            </RadioButton>

            <RadioButton Style="{StaticResource 标题_选项_样式}" x:Name="Home_Camera_Results" Grid.Column="3"
                         Margin="20,0,20,0"
                         VerticalAlignment="Stretch"
                         HorizontalContentAlignment="Center"
                         VerticalContentAlignment="Center"
                         GroupName="HandEye_Home">



                <StackPanel Orientation="Horizontal">

                    <Image Height=" 35"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center"
                           Effect="{DynamicResource 全局背景阴影}"
                           Source="/Resources/Vision_handEye_11.png" />
                    <TextBlock Text="标定结果"
                               Margin="20,0,0,0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontFamily="{DynamicResource 全局_文字_中文字体}"
                               FontSize="24"
                               Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                </StackPanel>

            </RadioButton>




        </Grid>

        <!--  分割装饰栏  -->
        <Border Grid.Row="1"
                Panel.ZIndex="-1"
                Background="{DynamicResource 全局_按钮_黑颜色}" />



        <Grid Grid.Row=" 3" Margin="0">
            <Grid.Style>
                <Style TargetType="Grid">

                    <Setter Property="Visibility" Value="Hidden" />
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding IsChecked, ElementName=Home_Camera_Para}" Value="True">
                            <Setter Property="Visibility" Value="Visible" />
                        </DataTrigger>
                    </Style.Triggers>

                </Style>
            </Grid.Style>


            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>


            <Border Grid.Row=" 0" Grid.Column="0"
                    MinWidth=" 280"
                    Margin="5"
                    Background="{DynamicResource 全局_背景_白颜色}"
                    BorderBrush="{DynamicResource 全局_按钮_蓝颜色}"
                    BorderThickness="3"
                    CornerRadius=" 10"
                    Effect="{DynamicResource 全局背景阴影}">

                <Grid>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="50" />
                        <RowDefinition />
                    </Grid.RowDefinitions>

                    <Border x:Name="Radio_1" Grid.Row=" 0" Grid.Column="1"
                            Height=" 50"
                            Margin="0"
                            Background="{DynamicResource 全局_按钮_蓝颜色}"
                            CornerRadius="5 5 0 0">


                        <Grid Grid.Row="0">


                            <StackPanel Margin="10"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Center"
                                        Orientation="Horizontal">
                                <Border Margin="0,0,10,0"
                                        Background="{DynamicResource 全局_按钮_灰颜色}"
                                        CornerRadius=" 15">
                                    <Image Height=" 24"
                                           Margin="5"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           Source="/Resources/Control_相机设置_ioc.png" />
                                </Border>
                                <TextBlock Text="Camera 0 内参参数"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontFamily="{DynamicResource 全局_文字_数字字体}"
                                           FontSize="16"
                                           Foreground="{DynamicResource 全局_按钮_白颜色}" />
                            </StackPanel>


                        </Grid>

                    </Border>


                    <ScrollViewer Style="{DynamicResource Vision_Data_List_ScrollViewerStyle}" x:Name="scrollViewer" Grid.Row=" 1" Grid.Column="0"
                                  Margin="3"
                                  HorizontalContentAlignment="Stretch"
                                  VerticalContentAlignment="Stretch"
                                  Background="Transparent"
                                  CanContentScroll="False"
                                  PanningMode="Both"
                                  ScrollViewer.VerticalScrollBarVisibility="Auto"
                                  VirtualizingPanel.IsVirtualizing="True"
                                  VirtualizingPanel.ScrollUnit="Pixel"
                                  VirtualizingPanel.VirtualizationMode="Recycling">


                        <Grid>
                            <Border Background="{DynamicResource 全局_背景_白颜色}" CornerRadius=" 0 0 5 5" />
                            <StackPanel Orientation="Vertical">


                                <!--<Grid Height="35" Margin="8">

                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width=" 90" />

                                                <ColumnDefinition Width=" *" />

                                                <ColumnDefinition Width=" 35" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="设备模式"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="16"
                                                       Foreground="{DynamicResource 全局_按钮_黑颜色}" />



                                            <ToggleButton Grid.Column=" 1"
                                                          HorizontalContentAlignment="Stretch"
                                                          VerticalContentAlignment="Center"
                                                          Tag="Camera_0">
                                                <ToggleButton.Style>
                                                    <Style BasedOn="{StaticResource Window_ToggleButtonStyle}" TargetType="ToggleButton">
                                                        <Style.Triggers>
                                                            <Trigger Property="IsChecked" Value="True" />
                                                            <DataTrigger Binding="{Binding Camera_Calibration_0.HaneEye_Calibration_Diver_Model}" Value="{x:Static Halcon_Enum:Image_Diver_Model_Enum.Online}">
                                                                <Setter Property="IsChecked" Value="True" />
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{Binding Camera_Calibration_0.HaneEye_Calibration_Diver_Model}" Value="{x:Static Halcon_Enum:Image_Diver_Model_Enum.Local}">
                                                                <Setter Property="IsChecked" Value="False" />
                                                            </DataTrigger>
                                                        </Style.Triggers>
                                                    </Style>
                                                </ToggleButton.Style>
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="*" />
                                                    </Grid.ColumnDefinitions>
                                                    <Label Grid.Column=" 0"
                                                           Content="在线设备"
                                                           Padding="0,0,0,0"
                                                           HorizontalAlignment="Center"
                                                           HorizontalContentAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           FontSize=" 14" />

                                                    <Label Grid.Column=" 1"
                                                           Content="本地模式"
                                                           Padding="0,0,0,0"
                                                           HorizontalAlignment="Center"
                                                           HorizontalContentAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           FontSize=" 14" />
                                                </Grid>

                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="Unchecked">
                                                        <i:InvokeCommandAction Command="{Binding HandEye_Diver_Model_Select_Comm}"
                                                                               CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                               PassEventArgsToCommand="True" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>

                                            </ToggleButton>


                                            <Label Grid.Column=" 3"
                                                   Content=""
                                                   Margin="0"
                                                   Padding="0"
                                                   HorizontalContentAlignment="Center"
                                                   VerticalContentAlignment="Center"
                                                   FontSize=" 14" />


                                        </Grid>-->

                                <Grid Height="40" Margin="8">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width=" 90" />

                                        <ColumnDefinition Width=" *" />

                                        <ColumnDefinition Width=" 35" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Text="设备模式"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_中文字体}"
                                               FontSize="16"
                                               Foreground="{DynamicResource 全局_按钮_黑颜色}" />



                                    <Border Grid.Column=" 1"
                                            Margin="0,0,0,0"
                                            BorderBrush="{DynamicResource 全局_按钮_黑颜色}"
                                            BorderThickness="2.5"
                                            CornerRadius=" 6">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition />
                                                <ColumnDefinition />

                                            </Grid.ColumnDefinitions>

                                            <RadioButton Style="{DynamicResource Function_ToggleButtonStyle}" x:Name="Camera_0_Diver" Grid.Column=" 0"
                                                         Content="在线设备"
                                                         IsChecked="{Binding Camera_Device_List.Camera_Diver_Model, Converter={StaticResource Radio_CheckedToEnum}, ConverterParameter={x:Static Halcon_Enum:Image_Diver_Model_Enum.Online}}"
                                                         FontSize=" 14"
                                                         GroupName="Camera_0_Diver"
                                                         Tag="Camera_0" />
                                            <RadioButton Style="{DynamicResource Function_ToggleButtonStyle}" Grid.Column=" 1"
                                                         Content="本地模式"
                                                         IsChecked="{Binding Camera_Device_List.Camera_Diver_Model, Converter={StaticResource Radio_CheckedToEnum}, ConverterParameter={x:Static Halcon_Enum:Image_Diver_Model_Enum.Local}}"
                                                         FontSize="14"
                                                         GroupName="Camera_0_Diver"
                                                         Tag="Camera_0">
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="Checked">
                                                        <i:InvokeCommandAction Command="{Binding HandEye_Diver_Model_Select_Comm}"
                                                                               CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                               PassEventArgsToCommand="True" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                            </RadioButton>

                                        </Grid>

                                    </Border>

                                    <Label Grid.Column=" 3"
                                           Content=""
                                           Margin="0"
                                           Padding="0"
                                           HorizontalContentAlignment="Center"
                                           VerticalContentAlignment="Center"
                                           FontSize=" 14" />


                                </Grid>



                                <Grid Height=" 35" Margin="8">
                                    <Grid.Style>
                                        <Style TargetType="Grid">
                                            <Setter Property="Visibility" Value="Collapsed" />
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding IsChecked, ElementName=Camera_0_Diver}" Value="false">
                                                    <Setter Property="Visibility" Value="Visible" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Grid.Style>

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width=" 90" />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition Width=" 35" />
                                    </Grid.ColumnDefinitions>


                                    <TextBlock Text="本地操作"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_中文字体}"
                                               FontSize="16"
                                               Foreground="{DynamicResource 全局_按钮_黑颜色}" />

                                    <Button Style="{DynamicResource Halcon_Window_Button}" Grid.Column=" 1"
                                            Margin="0,0,5,0"
                                            Background="{DynamicResource 全局_按钮_蓝颜色}"
                                            FontSize=" 26"
                                            Tag="Camera_0">
                                        <TextBlock Text="加载图像"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                   FontSize="14"
                                                   Foreground="{DynamicResource 全局_按钮_白颜色}" />



                                        <i:Interaction.Triggers>

                                            <i:EventTrigger EventName="Click">
                                                <i:InvokeCommandAction Command="{Binding HandEye_Local_Image_Mode_Comm}"
                                                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                       PassEventArgsToCommand="True" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </Button>






                                    <Button Style="{DynamicResource Halcon_Window_Button}" Grid.Column=" 2"
                                            Margin="5,0,0,0"
                                            Background="{DynamicResource 全局_按钮_蓝颜色}"
                                            Tag="Camera_0">


                                        <TextBlock Text="保存图像"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                   FontSize="14"
                                                   Foreground="{DynamicResource 全局_按钮_白颜色}" />

                                        <i:Interaction.Triggers>

                                            <i:EventTrigger EventName="Click">
                                                <i:InvokeCommandAction Command="{Binding HandEye_Save_Image_Mode_Comm}"
                                                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                       PassEventArgsToCommand="True" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>

                                    </Button>

                                </Grid>


                                <Grid Height=" 35" Margin="8">
                                    <Grid.Style>
                                        <Style TargetType="Grid">
                                            <Setter Property="Visibility" Value="Visible" />
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding IsChecked, ElementName=Camera_0_Diver}" Value="false">
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Grid.Style>


                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="90" />
                                        <ColumnDefinition />
                                        <ColumnDefinition Width=" 35" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Column=" 0"
                                               Text="配置文件"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_中文字体}"
                                               FontSize="16"
                                               Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                    <ComboBox Style="{DynamicResource Data_ComboBoxStyle}" Grid.Column=" 1"
                                              HorizontalContentAlignment="Left"
                                              VerticalContentAlignment="Center"
                                              BorderBrush="{DynamicResource 全局_按钮_黑颜色}"
                                              BorderThickness="2"
                                              IsDropDownOpen="False"
                                              IsSynchronizedWithCurrentItem="True"
                                              ItemsSource="{Binding Halcon_3DStereoModel.TwoCamera_Calibration_HCameraSetupModel_List}"
                                              SelectedIndex="0"
                                              SelectedValue="{Binding Halcon_3DStereoModel.Select_TwoCamera_Calibration_HCameraSetupMode}">
                                        <ComboBox.ItemTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding Fold.Name}"
                                                           TextWrapping="Wrap"
                                                           Padding="0,4,0,4"
                                                           FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                           FontSize="13"
                                                           Foreground="{DynamicResource 全局_按钮_黑颜色}"
                                                           TextTrimming="CharacterEllipsis" />
                                            </DataTemplate>
                                        </ComboBox.ItemTemplate>

                                        <i:Interaction.Triggers>

                                            <i:EventTrigger EventName="SelectionChanged">
                                                <i:InvokeCommandAction Command="{Binding Select_TwoCamera_Calibration_Command}"
                                                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                       PassEventArgsToCommand="True" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>


                                    </ComboBox>
                                </Grid>


                                <Grid Height="35" Margin="8">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width=" 90" />

                                        <ColumnDefinition />
                                        <ColumnDefinition Width=" 35" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Text="标定日期"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_中文字体}"
                                               FontSize="16"
                                               Foreground="{DynamicResource 全局_按钮_黑颜色}" />


                                    <TextBlock Grid.Column=" 1"
                                               Text="{Binding Halcon_3DStereoModel.Select_TwoCamera_Calibration_HCameraSetupMode.Fold.LastWriteTime, FallbackValue=..., StringFormat={}{0:yyyy-MM-dd HH:mm}}"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Center"
                                               Background="Transparent"
                                               FontSize="14" />
                                    <Label Grid.Column=" 3"
                                           Content=""
                                           Margin="0"
                                           Padding="0"
                                           HorizontalContentAlignment="Center"
                                           VerticalContentAlignment="Center"
                                           FontSize=" 14" />
                                </Grid>

                                <Grid Height="35" Margin="8">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width=" 90" />

                                        <ColumnDefinition />
                                        <ColumnDefinition Width=" 35" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Text="标定误差"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_中文字体}"
                                               FontSize="16"
                                               Foreground="{DynamicResource 全局_按钮_黑颜色}" />


                                    <TextBlock Grid.Column=" 1"
                                               Text="{Binding Halcon_3DStereoModel.Select_TwoCamera_Calibration_HCameraSetupMode.Camera_Calib_Error, StringFormat={}{0:F4}}"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Center"
                                               Background="Transparent"
                                               FontSize="14" />
                                    <Label Grid.Column=" 3"
                                           Content="Pixel"
                                           Margin="0"
                                           Padding="0"
                                           HorizontalContentAlignment="Center"
                                           VerticalContentAlignment="Center"
                                           FontSize=" 14" />
                                </Grid>


                                <Grid Height=" 35" Margin="8">
                                    <Grid.Style>
                                        <Style TargetType="Grid">
                                            <Setter Property="Visibility" Value="Collapsed" />
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding IsChecked, ElementName=Camera_0_Diver}" Value="false">
                                                    <Setter Property="Visibility" Value="Visible" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Grid.Style>


                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="90" />
                                        <ColumnDefinition />
                                        <ColumnDefinition Width=" 35" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Column=" 0"
                                               Text="相机选择"
                                               HorizontalAlignment="Left"
