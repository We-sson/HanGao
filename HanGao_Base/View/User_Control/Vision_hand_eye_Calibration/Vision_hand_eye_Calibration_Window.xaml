                                                        <Style.Triggers>
                                                            <DataTrigger Binding="{Binding IsChecked, ElementName=Camera_0_Diver}" Value="false">
                                                                <Setter Property="IsEnabled" Value="True" />
                                                            </DataTrigger>
                                                        </Style.Triggers>
                                                    </Style>
                                                </ComboBox.Style>

                                                <ComboBoxItem>

                                                    <TextBlock Text="面扫描(普通式)"
                                                               TextWrapping="Wrap"
                                                               Padding="0,4,0,4"
