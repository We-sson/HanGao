<Window x:Class="HanGao.View.User_Control.Vision_hand_eye_Calibration.Vision_hand_eye_Calibration_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:EnumModel="clr-namespace:Halcon_SDK_DLL;assembly=Halcon_SDK_DLL"
        xmlns:Enum_Converter="clr-namespace:Halcon_SDK_DLL.WPF_Converter;assembly=Halcon_SDK_DLL"
        xmlns:Halcon="http://schemas.mvtec.com/halcondotnet"
        xmlns:Halcon_Enum="clr-namespace:Halcon_SDK_DLL.Model;assembly=Halcon_SDK_DLL"
        xmlns:MVS="clr-namespace:MvCamCtrl.NET.CameraParams;assembly=MvCamCtrl.Net"
        xmlns:MVS_Enum="clr-namespace:MVS_SDK_Base.Model;assembly=MVS_SDK"
        xmlns:Openfile="clr-namespace:HanGao.View.User_Control.OpenFile"
        xmlns:Robot="clr-namespace:KUKA_Socket.Models;assembly=KUKA_Socket"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
        xmlns:local="clr-namespace:HanGao.View.User_Control.Vision_hand_eye_Calibration"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        Title="视觉手眼标定工具"
        MinWidth=" 1350"
        d:DataContext="{Binding Source={StaticResource Locator}, Path=Vision_hand_eye_Calibration_VM}"
        d:Height="1080"
        d:Width="1920"
        Background="{DynamicResource 全局_背景_白颜色}"
        mc:Ignorable="d">
    <Window.Resources>
        <Enum_Converter:Halcon_EnumConverter x:Key="Halcon_EnumConverter" />
        <Enum_Converter:Radio_CheckedToEnumConverter x:Key="Radio_CheckedToEnum" />
        <ObjectDataProvider x:Key="Robot_Enum"
                            MethodName="GetValues"
                            ObjectType="{x:Type sys:Enum}">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="Robot:Socket_Robot_Protocols_Enum" />
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
    </Window.Resources>

    <i:Interaction.Triggers>
        <i:EventTrigger EventName="Loaded">
            <i:InvokeCommandAction Command="{Binding Initialization_HandEye_Window_Comm}"
                                   CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                   PassEventArgsToCommand="True" />
        </i:EventTrigger>
        <!--<i:EventTrigger EventName="Closed">
            <i:InvokeCommandAction Command="{Binding Closed_Window_Dsipos_Comm}"
                                  CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                  PassEventArgsToCommand="True" />
        </i:EventTrigger>-->
    </i:Interaction.Triggers>

    <Grid>

        <TextBlock Text="&#xe640;"
                   Margin="15,10,0,0"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   FontFamily="/Resources/#iconfont"
                   FontSize="30"
                   Foreground="{DynamicResource 全局_按钮_蓝颜色}" />



        <TabControl Style="{DynamicResource TabControl_Style}" x:Name="Tab_Window" Grid.Row="0" Grid.Column=" 0"
                    Grid.ColumnSpan="2"
                    Margin="0,0,0,0"
                    Padding="0"
                    Background="Transparent"
                    BorderBrush="{DynamicResource 全局_按钮_蓝颜色}"
                    BorderThickness="0,4,0,0"
                    SelectedIndex="0"
                    TabStripPlacement="Top">
            <!--<i:Interaction.Triggers>
                <i:EventTrigger EventName="Loaded">
                    <i:InvokeCommandAction Command="{Binding Initialization_Halcon_UserControl_Comm}"
                                           CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                           PassEventArgsToCommand="True" />
                </i:EventTrigger>
            </i:Interaction.Triggers>-->

            <TabItem Style="{DynamicResource Vision_TabItemStyle}"
                     Padding="100,0,100,0"
                     Background="Transparent"
                     FontSize=" 22"
                     Header="设备参数">



                <Grid Margin="10">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>


                    <Border Grid.Row=" 0" Grid.Column="0"
                            MinWidth=" 280"
                            Margin="10"
                            Padding="4"
                            Background="{DynamicResource 全局_背景_白颜色}"
                            CornerRadius=" 10"
                            Effect="{DynamicResource 全局背景阴影}">

                        <Grid>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="50" />
                                <RowDefinition />
                            </Grid.RowDefinitions>

                            <Border x:Name="Radio_1" Grid.Row=" 0" Grid.Column="1"
                                    Height=" 50"
                                    Margin="0"
                                    Background="{DynamicResource 全局_按钮_蓝颜色}"
                                    CornerRadius="5 5 0 0">


                                <Grid Grid.Row="0">


                                    <StackPanel Margin="10"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Orientation="Horizontal">
                                        <Border Margin="0,0,10,0"
                                                Background="{DynamicResource 全局_按钮_灰颜色}"
                                                CornerRadius=" 15">
                                            <Image Height=" 24"
                                                   Margin="5"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   Source="/Resources/Control_相机设置_ioc.png" />
                                        </Border>
                                        <TextBlock Text="Camera 0 内参参数"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                   FontSize="16"
                                                   Foreground="{DynamicResource 全局_按钮_白颜色}" />
                                    </StackPanel>


                                </Grid>

                            </Border>


                            <ScrollViewer Style="{DynamicResource Vision_Data_List_ScrollViewerStyle}" x:Name="scrollViewer" Grid.Row=" 1" Grid.Column="0"
                                          HorizontalContentAlignment="Stretch"
                                          VerticalContentAlignment="Stretch"
                                          Background="Transparent"
                                          CanContentScroll="False"
                                          PanningMode="Both"
                                          ScrollViewer.VerticalScrollBarVisibility="Auto"
                                          VirtualizingPanel.IsVirtualizing="True"
                                          VirtualizingPanel.ScrollUnit="Pixel"
                                          VirtualizingPanel.VirtualizationMode="Recycling">


                                <Grid>
                                    <Border Background="{DynamicResource 全局_背景_白颜色}" CornerRadius=" 0 0 5 5" />
                                    <StackPanel Orientation="Vertical">


                                        <Grid Height=" 35" Margin="8">

                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="90" />
                                                <ColumnDefinition />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column=" 0"
                                                       Text="相机选择"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="16"
                                                       Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                            <ComboBox Style="{DynamicResource Data_ComboBoxStyle}" x:Name="Camera_0" Grid.Column=" 1"
                                                      HorizontalContentAlignment="Left"
                                                      VerticalContentAlignment="Center"
                                                      BorderBrush="{DynamicResource 全局_按钮_黑颜色}"
                                                      BorderThickness="2"
                                                      FontSize="20"
                                                      IsDropDownOpen="False"
                                                      IsSynchronizedWithCurrentItem="True"
                                                      ItemsSource="{Binding Camera_Calibration_Info_List}"
                                                      SelectedIndex=" -1"
                                                      SelectedValue="{Binding Camera_0_Select_Val}">
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="SelectionChanged">
                                                        <i:InvokeCommandAction Command="{Binding Selected_Camera_Paramer_Comm}"
                                                                               CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                               PassEventArgsToCommand="True" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>



                                                <ComboBox.ItemTemplate>
                                                    <DataTemplate>
                                                        <StackPanel Orientation="Horizontal">

                                                            <Border Width=" 23"
                                                                    Height=" 23"
                                                                    Margin="10,4,4,4"
                                                                    BorderThickness="2"
                                                                    CornerRadius=" 5">
                                                                <Border.Style>
                                                                    <Style TargetType="Border">
                                                                        <Setter Property="BorderBrush" Value="{DynamicResource 全局_按钮_黑颜色}" />
                                                                        <Setter Property="Background" Value="{DynamicResource 全局_按钮_黄颜色}" />
                                                                        <Style.Triggers>
                                                                            <DataTrigger Binding="{Binding Camer_Status, Mode=TwoWay}" Value="{x:Static MVS_Enum:MV_CAM_Device_Status_Enum.Possess}">
                                                                                <Setter Property="Background" Value="{DynamicResource 全局_按钮_红颜色}" />
                                                                            </DataTrigger>
                                                                            <DataTrigger Binding="{Binding Camer_Status, Mode=TwoWay}" Value="{x:Static MVS_Enum:MV_CAM_Device_Status_Enum.Connecting}">
                                                                                <Setter Property="Background" Value="{DynamicResource 全局_按钮_绿颜色}" />
                                                                            </DataTrigger>
                                                                            <DataTrigger Binding="{Binding IsSelected, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type DataGridRow}}}" Value="true">
                                                                                <Setter Property="BorderBrush" Value="{DynamicResource 全局_按钮_灰颜色}" />
                                                                            </DataTrigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </Border.Style>
                                                            </Border>

                                                            <TextBlock Text="{Binding Camera_Info.SerialNumber}"
                                                                       TextWrapping="Wrap"
                                                                       Margin="10,0,0,0"
                                                                       Padding="0,4,0,4"
                                                                       HorizontalAlignment="Center"
                                                                       VerticalAlignment="Center"
                                                                       FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                                       FontSize="16 "
                                                                       Foreground="{DynamicResource 全局_按钮_黑颜色}"
                                                                       IsEnabled="True"
                                                                       TextTrimming="CharacterEllipsis" />
                                                        </StackPanel>



                                                    </DataTemplate>
                                                </ComboBox.ItemTemplate>





                                            </ComboBox>


                                        </Grid>


                                        <Grid Height=" 35" Margin="8">

                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="90" />
                                                <ColumnDefinition />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column=" 0"
                                                       Text="相机类型"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="16"
                                                       Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                            <ComboBox Style="{DynamicResource Data_ComboBoxStyle}" x:Name="CameraCalibrationModel_0" Grid.Column=" 1"
                                                      HorizontalContentAlignment="Left"
                                                      VerticalContentAlignment="Center"
                                                      BorderBrush="{DynamicResource 全局_按钮_黑颜色}"
                                                      BorderThickness="2"
                                                      FontSize="20"
                                                      IsDropDownOpen="False"
                                                      IsEnabled="False"
                                                      IsSynchronizedWithCurrentItem="True"
                                                      SelectedIndex="{Binding Camera_Calibration_0.Camera_Calibration_Paramteters.Camera_Calibration_Model, Converter={StaticResource Halcon_EnumConverter}, ConverterParameter={x:Type Halcon_Enum:Halocn_Camera_Calibration_Enum}}">

                                                <ComboBoxItem>

                                                    <TextBlock Text="面扫描(普通式)"
                                                               TextWrapping="Wrap"
                                                               Padding="0,4,0,4"
                                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                               FontSize="14"
                                                               Foreground="{DynamicResource 全局_按钮_黑颜色}"
                                                               TextTrimming="CharacterEllipsis" />
                                                </ComboBoxItem>
                                                <ComboBoxItem>

                                                    <TextBlock Text="面扫描(多项式)"
                                                               TextWrapping="Wrap"
                                                               Padding="0,4,0,4"
                                                               FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                               FontSize="14"
                                                               Foreground="{DynamicResource 全局_按钮_黑颜色}"
                                                               TextTrimming="CharacterEllipsis" />
                                                </ComboBoxItem>


                                            </ComboBox>


                                        </Grid>



                                        <Grid Height="35" Margin="8">

                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width=" 90" />
                                                <ColumnDefinition />
                                                <ColumnDefinition Width=" auto" />
                                                <ColumnDefinition Width=" 35" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="焦距"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="16"
                                                       Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                            <Slider Style="{DynamicResource Vision_SliderStyle}" x:Name="Calibration_Focus_UI_0" Grid.Column=" 1"
                                                    Margin="5"
                                                    VerticalAlignment="Center"
                                                    HorizontalContentAlignment="Stretch"
                                                    VerticalContentAlignment="Stretch"
                                                    IsSnapToTickEnabled="True"
                                                    Maximum="50000"
                                                    Minimum="0"
                                                    SmallChange="10"
                                                    Value="{Binding Camera_Calibration_0.Camera_Calibration_Paramteters.Focus}" />

                                            <TextBox Grid.Column=" 2"
                                                     Text="{Binding ElementName=Calibration_Focus_UI_0, Path=Value, StringFormat={}{0:F4}}"
                                                     Width="100"
                                                     HorizontalContentAlignment="Center"
                                                     VerticalContentAlignment="Center"
                                                     Background="Transparent"
                                                     BorderBrush="{DynamicResource 全局_按钮_黑颜色}"
                                                     BorderThickness="2"
                                                     FontSize="18" />
                                            <Label Grid.Column=" 3"
                                                   Content="mm"
                                                   Margin="0"
                                                   Padding="0"
                                                   HorizontalContentAlignment="Center"
                                                   VerticalContentAlignment="Center"
                                                   FontSize=" 14" />
                                        </Grid>

                                        <Grid Height="35" Margin="8">

                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width=" 90" />
                                                <ColumnDefinition />
                                                <ColumnDefinition Width=" auto" />
                                                <ColumnDefinition Width=" 35" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="Kappa"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="16"
                                                       Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                            <Slider Style="{DynamicResource Vision_SliderStyle}" x:Name="Camera_Kappa_UI_0" Grid.Column=" 1"
                                                    Margin="5"
                                                    VerticalAlignment="Center"
                                                    HorizontalContentAlignment="Stretch"
                                                    VerticalContentAlignment="Stretch"
                                                    IsSnapToTickEnabled="True"
                                                    Maximum="999999999"
                                                    Minimum="-999999999"
                                                    SmallChange="10"
                                                    Value="{Binding Camera_Calibration_0.Camera_Calibration_Paramteters.Kappa}" />

                                            <TextBox Grid.Column=" 2"
                                                     Text="{Binding ElementName=Camera_Kappa_UI_0, Path=Value, StringFormat={}{0:F4}}"
                                                     Width="100"
                                                     HorizontalContentAlignment="Center"
                                                     VerticalContentAlignment="Center"
                                                     Background="Transparent"
                                                     BorderBrush="{DynamicResource 全局_按钮_黑颜色}"
                                                     BorderThickness="2"
                                                     FontSize="18" />
                                            <Label Grid.Column=" 3"
                                                   Content="1/㎡"
                                                   Margin="0"
                                                   Padding="0"
                                                   HorizontalContentAlignment="Center"
                                                   VerticalContentAlignment="Center"
                                                   FontSize=" 14" />
                                        </Grid>

                                        <Grid Height="35" Margin="8">

                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width=" 90" />
                                                <ColumnDefinition />
                                                <ColumnDefinition Width=" auto" />
                                                <ColumnDefinition Width=" 35" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="K1"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="16"
                                                       Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                            <Slider Style="{DynamicResource Vision_SliderStyle}" x:Name="Calibration_K1_UI_0" Grid.Column=" 1"
