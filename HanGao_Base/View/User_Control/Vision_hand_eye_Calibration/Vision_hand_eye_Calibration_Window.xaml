                            CornerRadius=" 10"
                            Effect="{DynamicResource 全局背景阴影}">

                        <Grid>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="50" />
                                <RowDefinition />
                            </Grid.RowDefinitions>

                            <Border x:Name="Radio_1" Grid.Row=" 0" Grid.Column="1"
