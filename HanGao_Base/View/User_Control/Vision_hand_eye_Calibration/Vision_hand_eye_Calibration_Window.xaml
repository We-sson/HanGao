                                                                 GroupName="Camera_0_Diver"
                                                                 Tag="Camera_0">
                                                        <i:Interaction.Triggers>
                                                            <i:EventTrigger EventName="Checked">
                                                                <i:InvokeCommandAction Command="{Binding HandEye_Diver_Model_Select_Comm}"
                                                                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                                       PassEventArgsToCommand="True" />
                                                            </i:EventTrigger>
                                                        </i:Interaction.Triggers>
                                                    </RadioButton>

                                                </Grid>

                                            </Border>

                                            <Label Grid.Column=" 3"
                                                   Content=""
                                                   Margin="0"
                                                   Padding="0"
                                                   HorizontalContentAlignment="Center"
                                                   VerticalContentAlignment="Center"
                                                   FontSize=" 14" />


                                        </Grid>



                                        <Grid Height=" 35" Margin="8">
                                            <Grid.Style>
                                                <Style TargetType="Grid">
                                                    <Setter Property="IsEnabled" Value="False" />
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding IsChecked, ElementName=Camera_0_Diver}" Value="false">
                                                            <Setter Property="IsEnabled" Value="True" />
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Grid.Style>

                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width=" 90" />
                                                <ColumnDefinition />
                                                <ColumnDefinition />
                                                <ColumnDefinition Width=" 35" />
                                            </Grid.ColumnDefinitions>


                                            <TextBlock Text="本地操作"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="16"
                                                       Foreground="{DynamicResource 全局_按钮_黑颜色}" />

                                            <Button Style="{DynamicResource Halcon_Window_Button}" Grid.Column=" 1"
                                                    Margin="0,0,5,0"
                                                    Background="{DynamicResource 全局_按钮_蓝颜色}"
                                                    FontSize=" 26"
                                                    Tag="Camera_0">
                                                <TextBlock Text="加载图像"
                                                           HorizontalAlignment="Center"
                                                           VerticalAlignment="Center"
                                                           FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                           FontSize="14"
                                                           Foreground="{DynamicResource 全局_按钮_白颜色}" />



                                                <i:Interaction.Triggers>

                                                    <i:EventTrigger EventName="Click">
                                                        <i:InvokeCommandAction Command="{Binding HandEye_Local_Image_Mode_Comm}"
                                                                               CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                               PassEventArgsToCommand="True" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                            </Button>






                                            <Button Style="{DynamicResource Halcon_Window_Button}" Grid.Column=" 2"
                                                    Margin="5,0,0,0"
                                                    Background="{DynamicResource 全局_按钮_蓝颜色}"
                                                    Tag="Camera_0">


                                                <TextBlock Text="保存图像"
                                                           HorizontalAlignment="Center"
                                                           VerticalAlignment="Center"
                                                           FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                           FontSize="14"
                                                           Foreground="{DynamicResource 全局_按钮_白颜色}" />

                                                <i:Interaction.Triggers>

                                                    <i:EventTrigger EventName="Click">
                                                        <i:InvokeCommandAction Command="{Binding HandEye_Save_Image_Mode_Comm}"
                                                                               CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                               PassEventArgsToCommand="True" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>

                                            </Button>

                                        </Grid>

                                        <Grid Height=" 35" Margin="8">

                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="90" />
                                                <ColumnDefinition />
                                                <ColumnDefinition Width=" 35" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column=" 0"
                                                       Text="相机选择"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="16"
                                                       Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                            <ComboBox x:Name="Camera_0" Grid.Column=" 1"
                                                      HorizontalContentAlignment="Left"
                                                      VerticalContentAlignment="Center"
                                                      FontSize="20"
                                                      IsDropDownOpen="False"
                                                      IsSynchronizedWithCurrentItem="True"
                                                      ItemsSource="{Binding Camera_Calibration_Info_List}"
                                                      SelectedIndex=" -1"
                                                      SelectedValue="{Binding Camera_0_Select_Val}">
                                                <ComboBox.Style>
                                                    <Style BasedOn="{StaticResource Data_ComboBoxStyle}" TargetType="ComboBox">
                                                        <Setter Property="IsEnabled" Value="True" />
                                                        <Style.Triggers>

                                                            <DataTrigger Binding="{Binding IsChecked, ElementName=Camera_0_Diver}" Value="false">
                                                                <Setter Property="IsEnabled" Value="False" />
                                                            </DataTrigger>
                                                        </Style.Triggers>
                                                    </Style>
                                                </ComboBox.Style>
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="SelectionChanged">
                                                        <i:InvokeCommandAction Command="{Binding Selected_Camera_Paramer_Comm}"
                                                                               CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                               PassEventArgsToCommand="True" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>



                                                <ComboBox.ItemTemplate>
                                                    <DataTemplate>
                                                        <StackPanel Orientation="Horizontal">

                                                            <Border Width=" 23"
                                                                    Height=" 23"
                                                                    Margin="10,4,4,4"
                                                                    BorderThickness="2"
                                                                    CornerRadius=" 5">
                                                                <Border.Style>
                                                                    <Style TargetType="Border">
                                                                        <Setter Property="BorderBrush" Value="{DynamicResource 全局_按钮_黑颜色}" />
                                                                        <Setter Property="Background" Value="{DynamicResource 全局_按钮_黄颜色}" />
                                                                        <Style.Triggers>
                                                                            <DataTrigger Binding="{Binding Camer_Status, Mode=TwoWay}" Value="{x:Static MVS_Enum:MV_CAM_Device_Status_Enum.Possess}">
                                                                                <Setter Property="Background" Value="{DynamicResource 全局_按钮_红颜色}" />
                                                                            </DataTrigger>
                                                                            <DataTrigger Binding="{Binding Camer_Status, Mode=TwoWay}" Value="{x:Static MVS_Enum:MV_CAM_Device_Status_Enum.Connecting}">
                                                                                <Setter Property="Background" Value="{DynamicResource 全局_按钮_绿颜色}" />
                                                                            </DataTrigger>

                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </Border.Style>
                                                            </Border>

                                                            <TextBlock Text="{Binding Camera_Info.SerialNumber}"
                                                                       TextWrapping="Wrap"
                                                                       Margin="10,0,0,0"
                                                                       Padding="0,4,0,4"
                                                                       HorizontalAlignment="Center"
                                                                       VerticalAlignment="Center"
                                                                       FontFamily="{DynamicResource 全局_文字_数字字体}"
                                                                       FontSize="16 "
                                                                       Foreground="{DynamicResource 全局_按钮_黑颜色}"
                                                                       IsEnabled="True"
                                                                       TextTrimming="CharacterEllipsis" />
                                                        </StackPanel>



                                                    </DataTemplate>
                                                </ComboBox.ItemTemplate>





                                            </ComboBox>



                                        </Grid>


                                        <Grid Height=" 35" Margin="8">

                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="90" />
                                                <ColumnDefinition />
                                                <ColumnDefinition Width=" 35" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column=" 0"
                                                       Text="相机类型"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="16"
                                                       Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                            <ComboBox x:Name="CameraCalibrationModel_0" Grid.Column=" 1"
                                                      HorizontalContentAlignment="Left"
                                                      VerticalContentAlignment="Center"
                                                      BorderBrush="{DynamicResource 全局_按钮_黑颜色}"
                                                      BorderThickness="2"
                                                      FontSize="20"
                                                      IsDropDownOpen="False"
                                                      IsSynchronizedWithCurrentItem="True"
                                                      SelectedIndex="{Binding Camera_Calibration_0.Camera_Calibration_Paramteters.Camera_Calibration_Model, Converter={StaticResource Halcon_EnumConverter}, ConverterParameter={x:Type Halcon_Enum:Halocn_Camera_Calibration_Enum}}">
                                                <ComboBox.Style>
                                                    <Style BasedOn="{StaticResource Data_ComboBoxStyle}" TargetType="ComboBox">
