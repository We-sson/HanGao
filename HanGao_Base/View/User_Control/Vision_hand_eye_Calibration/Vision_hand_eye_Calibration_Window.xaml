                                                                <Setter Property="IsChecked" Value="True" />
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{Binding Camera_Calibration_0.HaneEye_Calibration_Diver_Model}" Value="{x:Static Halcon_Enum:Image_Diver_Model_Enum.Local}">
                                                                <Setter Property="IsChecked" Value="False" />
                                                            </DataTrigger>
                                                        </Style.Triggers>
                                                    </Style>
                                                </ToggleButton.Style>
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="*" />
                                                    </Grid.ColumnDefinitions>
                                                    <Label Grid.Column=" 0"
                                                           Content="在线设备"
                                                           Padding="0,0,0,0"
                                                           HorizontalAlignment="Center"
                                                           HorizontalContentAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           FontSize=" 14" />

                                                    <Label Grid.Column=" 1"
                                                           Content="本地模式"
                                                           Padding="0,0,0,0"
                                                           HorizontalAlignment="Center"
                                                           HorizontalContentAlignment="Center"
                                                           VerticalContentAlignment="Center"
                                                           FontSize=" 14" />
                                                </Grid>

                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="Unchecked">
                                                        <i:InvokeCommandAction Command="{Binding HandEye_Diver_Model_Select_Comm}"
                                                                               CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                               PassEventArgsToCommand="True" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>

                                            </ToggleButton>


                                            <Label Grid.Column=" 3"
                                                   Content=""
                                                   Margin="0"
                                                   Padding="0"
                                                   HorizontalContentAlignment="Center"
                                                   VerticalContentAlignment="Center"
                                                   FontSize=" 14" />


                                        </Grid>-->

                                        <Grid Height="40" Margin="8">

                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width=" 90" />

                                                <ColumnDefinition Width=" *" />

                                                <ColumnDefinition Width=" 35" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="设备模式"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="16"
                                                       Foreground="{DynamicResource 全局_按钮_黑颜色}" />



                                            <Border Grid.Column=" 1"
