                                                    BorderBrush="{DynamicResource 全局_按钮_黑颜色}"
                                                    BorderThickness="2.5"
                                                    CornerRadius=" 6">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition />
                                                        <ColumnDefinition />

                                                    </Grid.ColumnDefinitions>

                                                    <RadioButton Style="{DynamicResource Function_ToggleButtonStyle}" x:Name="Camera_0_Diver" Grid.Column=" 0"
                                                                 Content="在线设备"
