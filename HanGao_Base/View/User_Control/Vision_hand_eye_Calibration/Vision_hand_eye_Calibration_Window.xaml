                                                       FontSize="16"
                                                       Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                            <Slider Style="{DynamicResource Vision_SliderStyle}" x:Name="Calibration_K2_UI_0" Grid.Column=" 1"
                                                    Margin="5"
                                                    VerticalAlignment="Center"
                                                    HorizontalContentAlignment="Stretch"
                                                    VerticalContentAlignment="Stretch"
                                                    IsSnapToTickEnabled="True"
                                                    Maximum="999999999"
                                                    Minimum="-999999999"
                                                    SmallChange="10"
                                                    Value="{Binding Camera_Calibration_0.Camera_Calibration_Paramteters.K2}" />

                                            <TextBox Grid.Column=" 2"
                                                     Text="{Binding ElementName=Calibration_K2_UI_0, Path=Value, StringFormat={}{0:F4}}"
                                                     Width="100"
                                                     HorizontalContentAlignment="Center"
                                                     VerticalContentAlignment="Center"
                                                     Background="Transparent"
                                                     BorderBrush="{DynamicResource 全局_按钮_黑颜色}"
                                                     BorderThickness="2"
                                                     FontSize="18" />
                                            <Label Grid.Column=" 3"
                                                   Content="1/㎡"
                                                   Margin="0"
                                                   Padding="0"
                                                   HorizontalContentAlignment="Center"
                                                   VerticalContentAlignment="Center"
                                                   FontSize=" 14" />
                                        </Grid>

                                        <Grid Height="35" Margin="8">

                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width=" 90" />
                                                <ColumnDefinition />
                                                <ColumnDefinition Width=" auto" />
                                                <ColumnDefinition Width=" 35" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="K3"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="16"
                                                       Foreground="{DynamicResource 全局_按钮_黑颜色}" />
                                            <Slider Style="{DynamicResource Vision_SliderStyle}" x:Name="Calibration_K3_UI_0" Grid.Column=" 1"
                                                    Margin="5"
                                                    VerticalAlignment="Center"
                                                    HorizontalContentAlignment="Stretch"
                                                    VerticalContentAlignment="Stretch"
                                                    IsSnapToTickEnabled="True"
                                                    Maximum="9999999999999"
                                                    Minimum="-9999999999999"
                                                    SmallChange="10"
                                                    Value="{Binding Camera_Calibration_0.Camera_Calibration_Paramteters.K3}" />

                                            <TextBox Grid.Column=" 2"
                                                     Text="{Binding ElementName=Calibration_K3_UI_0, Path=Value, StringFormat={}{0:F4}}"
                                                     Width="100"
                                                     HorizontalContentAlignment="Center"
                                                     VerticalContentAlignment="Center"
                                                     Background="Transparent"
                                                     BorderBrush="{DynamicResource 全局_按钮_黑颜色}"
                                                     BorderThickness="2"
                                                     FontSize="18" />
                                            <Label Grid.Column=" 3"
                                                   Content="1/㎡"
                                                   Margin="0"
                                                   Padding="0"
                                                   HorizontalContentAlignment="Center"
                                                   VerticalContentAlignment="Center"
                                                   FontSize=" 14" />
                                        </Grid>

                                        <Grid Height="35" Margin="8">

                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width=" 90" />
                                                <ColumnDefinition />
                                                <ColumnDefinition Width=" auto" />
                                                <ColumnDefinition Width=" 35" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="P1"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
