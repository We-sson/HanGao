                                        <TextBlock Grid.Row=" 12" Grid.Column=" 2"
                                                   Text="{Binding HandEye_Results.HandEye_Results_Pos.HandEye_Obj_In_Base_Pose_Deviations.Rz, StringFormat={}{0:F4}°}"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontSize="16" />
                                    </Grid>


                                </Border>


                            </StackPanel>

                        </Grid>

                    </ScrollViewer>

                </Grid>



            </Border>

            <Border Grid.Row=" 0" Grid.Column="3"
                    MinWidth=" 280"
                    Margin="5"
                    Background="{DynamicResource 全局_背景_白颜色}"
                    BorderBrush="{DynamicResource 全局_按钮_蓝颜色}"
                    BorderThickness="3"
                    CornerRadius=" 10"
                    Effect="{DynamicResource 全局背景阴影}">


                <Grid>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto" />
                        <RowDefinition />
                    </Grid.RowDefinitions>

                    <Border Grid.Row=" 0" Grid.Column="0"
                            Height=" 50"
                            Margin="0"
                            Background="{DynamicResource 全局_按钮_蓝颜色}"
                            CornerRadius="5 5 0 0">


                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width=" auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>


                            <Border Grid.Column=" 0"
                                    Margin="10,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Background="{DynamicResource 全局_按钮_灰颜色}"
                                    CornerRadius=" 15">
                                <Image Height=" 24"
                                       Margin="5"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       Source="/Resources/Vision_标定图标.png" />
                            </Border>
                            <TextBlock Grid.Column=" 0"
                                       Grid.ColumnSpan=" 2"
                                       Text="标定参数"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontFamily="{DynamicResource 全局_文字_数字字体}"
                                       FontSize="16"
                                       Foreground="{DynamicResource 全局_按钮_白颜色}" />



                        </Grid>

                    </Border>


                    <ScrollViewer Style="{DynamicResource Vision_Data_List_ScrollViewerStyle}" Grid.Row=" 1" Grid.Column="0"
                                  Margin="3"
                                  HorizontalContentAlignment="Stretch"
                                  VerticalContentAlignment="Stretch"
                                  Background="Transparent"
                                  CanContentScroll="False"
                                  PanningMode="Both"
                                  ScrollViewer.VerticalScrollBarVisibility="Auto"
                                  VirtualizingPanel.IsVirtualizing="True"
                                  VirtualizingPanel.ScrollUnit="Pixel"
                                  VirtualizingPanel.VirtualizationMode="Recycling">

                        <Grid>
                            <Border Background="{DynamicResource 全局_背景_白颜色}" CornerRadius=" 0 0 5 5" />

                            <StackPanel Orientation="Vertical">


                                <Grid Height=" 100" Margin="8">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="90" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="35" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Column=" 0"
                                               Text="手眼保存位置"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_中文字体}"
                                               FontSize="14"
                                               Foreground="{DynamicResource 全局_按钮_黑颜色}" />


                                    <Openfile:UC_Open_File Grid.Column=" 1"
                                                           File_Log="{Binding HandEye_Camera_Parameters.HandEye_Result_Fold_Address, Mode=TwoWay}"
                                                           File_Model="Folder" />


                                    <Label Grid.Column=" 3"
                                           Content=""
                                           Margin="0"
                                           Padding="0"
                                           HorizontalContentAlignment="Center"
                                           VerticalContentAlignment="Center"
                                           FontSize=" 14" />

                                </Grid>



                                <Grid Height="35" Margin="8">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="90" />
                                        <ColumnDefinition Width=" *" />

                                        <ColumnDefinition Width=" *" />
                                        <ColumnDefinition Width=" 35" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Text="功能操作"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Center"
                                               FontFamily="{DynamicResource 全局_文字_中文字体}"
                                               FontSize="16"
                                               Foreground="{DynamicResource 全局_按钮_黑颜色}" />


                                    <Button Grid.Column=" 1"
                                            Margin="0,0,4,0"
                                            HorizontalContentAlignment="Stretch"
                                            VerticalContentAlignment="Stretch"
                                            Background="{DynamicResource 全局_按钮_蓝颜色}"
                                            Tag="Camera_0">
                                        <Button.Resources>
                                            <ProgressBar x:Key="Calibration_PBar"
                                                         Margin="4"
                                                         BorderThickness="0"
                                                         IsIndeterminate="True"
                                                         Maximum=" 100"
                                                         Minimum=" 0" />

                                            <TextBlock x:Key="Calibration_Results_Txt"
                                                       Text="备用"
                                                       HorizontalAlignment="Center"
                                                       VerticalAlignment="Center"
                                                       FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                       FontSize="16"
                                                       Foreground="{DynamicResource 全局_按钮_白颜色}" />
                                        </Button.Resources>

                                        <Button.Style>
                                            <Style BasedOn="{StaticResource Halcon_Window_Button}" TargetType="Button">
                                                <Setter Property="Content" Value="{StaticResource Calibration_Results_Txt}" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding HandEye_Results.HandEye_Camera_0_Results.Camera_Calinration_Process_Type}" Value="{x:Static Halcon_Enum:Camera_Calinration_Process_Enum.Calibrationing}">
                                                        <Setter Property="Content" Value="{StaticResource Calibration_PBar}" />
                                                        <Setter Property="IsEnabled" Value="False" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Button.Style>

                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="Click">
                                                <i:InvokeCommandAction Command="{Binding}"
                                                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                       PassEventArgsToCommand="True" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </Button>



                                    <Button Style="{DynamicResource Halcon_Window_Button}" Grid.Column=" 2"
                                            Margin="4,0,0,0"
                                            Background="{DynamicResource 全局_按钮_蓝颜色}"
                                            Tag="Camera_0">
                                        <TextBlock Text="保存文件"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontFamily="{DynamicResource 全局_文字_中文字体}"
                                                   FontSize="16"
                                                   Foreground="{DynamicResource 全局_按钮_白颜色}" />
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="Click">
                                                <i:InvokeCommandAction Command="{Binding}"
                                                                       CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                                                       PassEventArgsToCommand="True" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </Button>




                                </Grid>















                            </StackPanel>

                        </Grid>


                    </ScrollViewer>

                </Grid>



            </Border>


        </Grid>

        <TabControl Style="{DynamicResource TabControl_Style}" x:Name="Tab_Window" Grid.Row="0" Grid.Column=" 0"
                    Grid.ColumnSpan="2"
                    Margin="0,0,0,0"
                    Padding="0"
                    HorizontalContentAlignment="Center"
                    Background="Transparent"
                    BorderBrush="{DynamicResource 全局_按钮_蓝颜色}"
                    BorderThickness="0,4,0,0"
                    SelectedIndex="0"
                    TabStripPlacement="Top"
                    Visibility="Collapsed">
            <!--<i:Interaction.Triggers>
                <i:EventTrigger EventName="Loaded">
                    <i:InvokeCommandAction Command="{Binding Initialization_Halcon_UserControl_Comm}"
                                           CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}, Path=EventArgsConverter}"
                                           PassEventArgsToCommand="True" />
                </i:EventTrigger>
            </i:Interaction.Triggers>-->

            <TabItem Style="{DynamicResource Vision_TabItemStyle}"
                     Padding="50,8,50,8"
                     Background="Transparent"
                     FontSize=" 22"
                     Header="① 设备参数" />
            <TabItem Style="{DynamicResource Vision_TabItemStyle}"
                     Padding="50,8,50,8"
                     Background="Transparent"
                     FontSize=" 22"
                     Header="② 相机测试" />
            <TabItem Style="{DynamicResource Vision_TabItemStyle}"
                     Padding="50,8,50,8"
                     Background="Transparent"
                     FontSize=" 22"
                     Header="③ 图像采集计算" />

            <TabItem Style="{DynamicResource Vision_TabItemStyle}"
                     Padding="50,8,50,8"
                     Background="Transparent"
                     FontSize=" 22"
                     Header="④ 手眼标定结果" />


        </TabControl>

    </Grid>
</Window>
